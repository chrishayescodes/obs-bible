<ns1:verse osisID="Exod.40.1" sID="Exod.40.1" /><ns1:w lemma="strong:H03068">And the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01696">spake</ns1:w> <ns1:w lemma="strong:H04872">unto Moses</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>,<ns1:verse eID="Exod.40.1" />
