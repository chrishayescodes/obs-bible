<ns1:verse osisID="Exod.35.27" sID="Exod.35.27" /><ns1:w lemma="strong:H05387">And the rulers</ns1:w> <ns1:w morph="strongMorph:TH8689" lemma="strong:H0935">brought</ns1:w> <ns1:w lemma="strong:H07718">onyx</ns1:w> <ns1:w lemma="strong:H068">stones</ns1:w>, <ns1:w lemma="strong:H068">and stones</ns1:w> <ns1:w lemma="strong:H04394">to be set</ns1:w>, <ns1:w lemma="strong:H0646">for the ephod</ns1:w>, <ns1:w lemma="strong:H02833">and for the breastplate</ns1:w>;<ns1:verse eID="Exod.35.27" />
