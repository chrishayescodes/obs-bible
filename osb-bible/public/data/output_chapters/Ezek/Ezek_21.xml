<ns1:verse osisID="Ezek.21.9" sID="Ezek.21.9" /><ns1:w lemma="strong:H01121">Son</ns1:w> <ns1:w lemma="strong:H0120">of man</ns1:w>, <ns1:w morph="strongMorph:TH8734" lemma="strong:H05012">prophesy</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H0559">and say</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H0559">Thus saith</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>; <ns1:w morph="strongMorph:TH8798" lemma="strong:H0559">Say</ns1:w>, <ns1:w lemma="strong:H02719">A sword</ns1:w>, <ns1:w lemma="strong:H02719">a sword</ns1:w> <ns1:w morph="strongMorph:TH8717" lemma="strong:H02300">is sharpened</ns1:w>, <ns1:w morph="strongMorph:TH8803" lemma="strong:H04803">and also furbished</ns1:w>:<ns1:verse eID="Ezek.21.9" />
