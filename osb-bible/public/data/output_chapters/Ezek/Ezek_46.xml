<ns1:verse osisID="Ezek.46.13" sID="Ezek.46.13" /><ns1:w lemma="strong:H03117">Thou shalt daily</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06213">prepare</ns1:w> <ns1:w lemma="strong:H05930">a burnt offering</ns1:w> <ns1:w lemma="strong:H03068">unto the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:transChange type="added">of</ns1:transChange> <ns1:w lemma="strong:H03532">a lamb</ns1:w> <ns1:w lemma="strong:H01121">of the first</ns1:w> <ns1:w lemma="strong:H08141">year</ns1:w> <ns1:w lemma="strong:H08549">without blemish</ns1:w>: <ns1:w morph="strongMorph:TH8799" lemma="strong:H06213">thou shalt prepare</ns1:w> <ns1:w lemma="strong:H01242">it every</ns1:w> <ns1:w lemma="strong:H01242">morning</ns1:w>.<ns1:note type="study">of the…: Heb. a son of his year</ns1:note><ns1:note type="study">every…: Heb. morning by morning</ns1:note><ns1:verse eID="Ezek.46.13" />
