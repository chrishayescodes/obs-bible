<ns1:verse osisID="Josh.18.23" sID="Josh.18.23" /><ns1:w lemma="strong:H05761">And Avim</ns1:w>, <ns1:w lemma="strong:H06511">and Parah</ns1:w>, <ns1:w lemma="strong:H06084">and Ophrah</ns1:w>,<ns1:verse eID="Josh.18.23" />
<ns1:verse osisID="Josh.18.24" sID="Josh.18.24" /><ns1:w lemma="strong:H03726">And Chephar–haammonai</ns1:w>, <ns1:w lemma="strong:H06078">and Ophni</ns1:w>, <ns1:w lemma="strong:H01387">and Gaba</ns1:w>; <ns1:w lemma="strong:H06240 strong:H08147">twelve</ns1:w> <ns1:w lemma="strong:H05892">cities</ns1:w> <ns1:w lemma="strong:H02691">with their villages</ns1:w>:<ns1:verse eID="Josh.18.24" />
