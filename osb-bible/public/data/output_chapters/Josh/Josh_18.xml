<ns1:verse osisID="Josh.18.26" sID="Josh.18.26" /><ns1:w lemma="strong:H04708">And Mizpeh</ns1:w>, <ns1:w lemma="strong:H03716">and Chephirah</ns1:w>, <ns1:w lemma="strong:H04681">and Mozah</ns1:w>,<ns1:verse eID="Josh.18.26" />
