<ns1:verse osisID="Josh.10.17" sID="Josh.10.17" /><ns1:w morph="strongMorph:TH8714" lemma="strong:H05046">And it was told</ns1:w> <ns1:w lemma="strong:H03091">Joshua</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>, <ns1:w lemma="strong:H02568">The five</ns1:w> <ns1:w lemma="strong:H04428">kings</ns1:w> <ns1:w morph="strongMorph:TH8738" lemma="strong:H04672">are found</ns1:w> <ns1:w morph="strongMorph:TH8737" lemma="strong:H02244">hid</ns1:w> <ns1:w lemma="strong:H04631">in a cave</ns1:w> <ns1:w lemma="strong:H04719">at Makkedah</ns1:w>.<ns1:verse eID="Josh.10.17" />
