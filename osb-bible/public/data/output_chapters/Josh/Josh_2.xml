<ns1:verse osisID="Josh.2.12" sID="Josh.2.12" /><ns1:w morph="strongMorph:TH8734" lemma="strong:H07650">Now therefore, I pray you, swear</ns1:w> <ns1:w lemma="strong:H03068">unto me by the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>, <ns1:w lemma="strong:H03588">since</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H06213">I have shewed</ns1:w> <ns1:w lemma="strong:H02617">you kindness</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H06213">that ye will also shew</ns1:w> <ns1:w lemma="strong:H02617">kindness</ns1:w> <ns1:w lemma="strong:H01">unto my father's</ns1:w> <ns1:w lemma="strong:H01004">house</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H05414">and give</ns1:w> <ns1:w lemma="strong:H0571">me a true</ns1:w> <ns1:w lemma="strong:H0226">token</ns1:w>:<ns1:verse eID="Josh.2.12" />
<ns1:verse osisID="Josh.2.13" sID="Josh.2.13" />And <ns1:transChange type="added">that</ns1:transChange> <ns1:w morph="strongMorph:TH8689" lemma="strong:H02421">ye will save alive</ns1:w> <ns1:w lemma="strong:H01">my father</ns1:w>, <ns1:w lemma="strong:H0517">and my mother</ns1:w>, <ns1:w lemma="strong:H0251">and my brethren</ns1:w>, <ns1:w lemma="strong:H0269">and my sisters</ns1:w>, <ns1:w morph="strongMorph:TH8689" lemma="strong:H05337">and all that they have, and deliver</ns1:w> <ns1:w lemma="strong:H05315">our lives</ns1:w> <ns1:w lemma="strong:H04194">from death</ns1:w>.<ns1:verse eID="Josh.2.13" />
