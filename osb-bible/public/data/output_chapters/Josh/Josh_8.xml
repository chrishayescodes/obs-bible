<ns1:title type="chapter">CHAPTER 8.</ns1:title>
<ns1:verse osisID="Josh.8.1" sID="Josh.8.1" /><ns1:w lemma="strong:H03068">And the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w> <ns1:w lemma="strong:H03091">unto Joshua</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H03372">Fear</ns1:w> <ns1:w morph="strongMorph:TH8735" lemma="strong:H02865">not, neither be thou dismayed</ns1:w>: <ns1:w morph="strongMorph:TH8798" lemma="strong:H03947">take</ns1:w> <ns1:w lemma="strong:H05971">all the people</ns1:w> <ns1:w lemma="strong:H04421">of war</ns1:w> <ns1:w morph="strongMorph:TH8798" lemma="strong:H06965">with thee, and arise</ns1:w>, <ns1:w morph="strongMorph:TH8798" lemma="strong:H05927">go up</ns1:w> <ns1:w lemma="strong:H05857">to Ai</ns1:w>: <ns1:w morph="strongMorph:TH8798" lemma="strong:H07200">see</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H05414">I have given</ns1:w> <ns1:w lemma="strong:H03027">into thy hand</ns1:w> <ns1:w lemma="strong:H04428">the king</ns1:w> <ns1:w lemma="strong:H05857">of Ai</ns1:w>, <ns1:w lemma="strong:H05971">and his people</ns1:w>, <ns1:w lemma="strong:H05892">and his city</ns1:w>, <ns1:w lemma="strong:H0776">and his land</ns1:w>:<ns1:verse eID="Josh.8.1" />
<ns1:verse osisID="Josh.8.2" sID="Josh.8.2" /><ns1:w morph="strongMorph:TH8804" lemma="strong:H06213">And thou shalt do</ns1:w> <ns1:w lemma="strong:H05857">to Ai</ns1:w> <ns1:w lemma="strong:H04428">and her king</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H06213">as thou didst</ns1:w> <ns1:w lemma="strong:H03405">unto Jericho</ns1:w> <ns1:w lemma="strong:H04428">and her king</ns1:w>: <ns1:w lemma="strong:H07998">only the spoil</ns1:w> <ns1:w lemma="strong:H0929">thereof, and the cattle</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0962">thereof, shall ye take for a prey</ns1:w> <ns1:w morph="strongMorph:TH8798" lemma="strong:H07760">unto yourselves: lay</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H0693">thee an ambush</ns1:w> <ns1:w lemma="strong:H05892">for the city</ns1:w> <ns1:w lemma="strong:H0310">behind</ns1:w> it.<ns1:verse eID="Josh.8.2" />
