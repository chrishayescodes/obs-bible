<ns1:verse osisID="Deut.11.26" sID="Deut.11.26" /><ns1:milestone type="x-p" marker="¶" /><ns1:w morph="strongMorph:TH8798" lemma="strong:H07200">Behold</ns1:w>, <ns1:w morph="strongMorph:TH8802" lemma="strong:H05414">I set</ns1:w> <ns1:w lemma="strong:H06440">before</ns1:w> <ns1:w lemma="strong:H03117">you this day</ns1:w> <ns1:w lemma="strong:H01293">a blessing</ns1:w> <ns1:w lemma="strong:H07045">and a curse</ns1:w>;<ns1:verse eID="Deut.11.26" />
