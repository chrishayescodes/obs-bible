<ns1:verse osisID="Deut.11.29" sID="Deut.11.29" /><ns1:w lemma="strong:H03068">And it shall come to pass, when the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H0430">thy God</ns1:w> <ns1:w morph="strongMorph:TH8686" lemma="strong:H0935">hath brought thee in</ns1:w> <ns1:w lemma="strong:H0776">unto the land</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H0935">whither thou goest</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H03423">to possess</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H05414">it, that thou shalt put</ns1:w> <ns1:w lemma="strong:H01293">the blessing</ns1:w> <ns1:w lemma="strong:H02022">upon mount</ns1:w> <ns1:w lemma="strong:H01630">Gerizim</ns1:w>, <ns1:w lemma="strong:H07045">and the curse</ns1:w> <ns1:w lemma="strong:H02022">upon mount</ns1:w> <ns1:w lemma="strong:H05858">Ebal</ns1:w>.<ns1:verse eID="Deut.11.29" />
