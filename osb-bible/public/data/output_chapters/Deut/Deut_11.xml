<ns1:verse osisID="Deut.11.24" sID="Deut.11.24" /><ns1:w lemma="strong:H04725">Every place</ns1:w> <ns1:w lemma="strong:H03709">whereon the soles</ns1:w> <ns1:w lemma="strong:H07272">of your feet</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H01869">shall tread</ns1:w> <ns1:w lemma="strong:H04057">shall be yours: from the wilderness</ns1:w> <ns1:w lemma="strong:H03844">and Lebanon</ns1:w>, <ns1:w lemma="strong:H05104">from the river</ns1:w>, <ns1:w lemma="strong:H05104">the river</ns1:w> <ns1:w lemma="strong:H06578">Euphrates</ns1:w>, <ns1:w lemma="strong:H0314">even unto the uttermost</ns1:w> <ns1:w lemma="strong:H03220">sea</ns1:w> <ns1:w lemma="strong:H01366">shall your coast</ns1:w> be.<ns1:verse eID="Deut.11.24" />
