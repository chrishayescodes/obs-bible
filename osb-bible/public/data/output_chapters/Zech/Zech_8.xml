<ns1:title type="chapter">CHAPTER 8.</ns1:title>
<ns1:verse osisID="Zech.8.1" sID="Zech.8.1" /><ns1:w lemma="strong:H01697">Again the word</ns1:w> <ns1:w lemma="strong:H03068">of the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H06635">of hosts</ns1:w> came <ns1:transChange type="added">to me</ns1:transChange>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>,<ns1:verse eID="Zech.8.1" />
