<div type="book" osisID="Ps" canonical="true"><ns1:title type="main">THE BOOK OF PSALMS</ns1:title>
<ns1:chapter osisID="Ps.128" chapterTitle="PSALM 128.">
<ns1:title type="chapter">PSALM 128.</ns1:title>
<ns1:title type="psalm" canonical="true"><ns1:w lemma="strong:H07892">A Song</ns1:w> <ns1:w lemma="strong:H04609">of degrees</ns1:w>.</ns1:title><ns1:verse osisID="Ps.128.1" sID="Ps.128.1" /><ns1:w lemma="strong:H0835">Blessed</ns1:w> <ns1:transChange type="added">is</ns1:transChange> <ns1:w lemma="strong:H03373">every one that feareth</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>; <ns1:w morph="strongMorph:TH8802" lemma="strong:H01980">that walketh</ns1:w> <ns1:w lemma="strong:H01870">in his ways</ns1:w>.<ns1:verse eID="Ps.128.1" />
<ns1:verse osisID="Ps.128.2" sID="Ps.128.2" /><ns1:w morph="strongMorph:TH8799" lemma="strong:H0398">For thou shalt eat</ns1:w> <ns1:w lemma="strong:H03018">the labour</ns1:w> <ns1:w lemma="strong:H03709">of thine hands</ns1:w>: <ns1:w lemma="strong:H0835">happy</ns1:w> <ns1:transChange type="added">shalt</ns1:transChange> thou <ns1:transChange type="added">be</ns1:transChange>, and <ns1:transChange type="added">it shall be</ns1:transChange> <ns1:w lemma="strong:H02896">well</ns1:w> with thee.<ns1:verse eID="Ps.128.2" />
<ns1:verse osisID="Ps.128.3" sID="Ps.128.3" /><ns1:w lemma="strong:H0802">Thy wife</ns1:w> <ns1:transChange type="added">shall be</ns1:transChange> <ns1:w morph="strongMorph:TH8802" lemma="strong:H06509">as a fruitful</ns1:w> <ns1:w lemma="strong:H01612">vine</ns1:w> <ns1:w lemma="strong:H03411">by the sides</ns1:w> <ns1:w lemma="strong:H01004">of thine house</ns1:w>: <ns1:w lemma="strong:H01121">thy children</ns1:w> <ns1:w lemma="strong:H02132">like olive</ns1:w> <ns1:w lemma="strong:H08363">plants</ns1:w> <ns1:w lemma="strong:H05439">round about</ns1:w> <ns1:w lemma="strong:H07979">thy table</ns1:w>.<ns1:verse eID="Ps.128.3" />
<ns1:verse osisID="Ps.128.4" sID="Ps.128.4" /><ns1:w lemma="strong:H01397">Behold, that thus shall the man</ns1:w> <ns1:w morph="strongMorph:TH8792" lemma="strong:H01288">be blessed</ns1:w> <ns1:w lemma="strong:H03373">that feareth</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>.<ns1:verse eID="Ps.128.4" />
<ns1:verse osisID="Ps.128.5" sID="Ps.128.5" /><ns1:w lemma="strong:H03068">The <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01288">shall bless</ns1:w> <ns1:w lemma="strong:H06726">thee out of Zion</ns1:w>: <ns1:w morph="strongMorph:TH8798" lemma="strong:H07200">and thou shalt see</ns1:w> <ns1:w lemma="strong:H02898">the good</ns1:w> <ns1:w lemma="strong:H03389">of Jerusalem</ns1:w> <ns1:w lemma="strong:H03117">all the days</ns1:w> <ns1:w lemma="strong:H02416">of thy life</ns1:w>.<ns1:verse eID="Ps.128.5" />
<ns1:verse osisID="Ps.128.6" sID="Ps.128.6" /><ns1:w morph="strongMorph:TH8798" lemma="strong:H07200">Yea, thou shalt see</ns1:w> <ns1:w lemma="strong:H01121">thy children's</ns1:w> <ns1:w lemma="strong:H01121">children</ns1:w>, <ns1:transChange type="added">and</ns1:transChange> <ns1:w lemma="strong:H07965">peace</ns1:w> <ns1:w lemma="strong:H03478">upon Israel</ns1:w>.<ns1:verse eID="Ps.128.6" />
</ns1:chapter>
