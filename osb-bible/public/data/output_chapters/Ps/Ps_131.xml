<ns1:verse osisID="Ps.131.3" sID="Ps.131.3" /><ns1:w lemma="strong:H03478">Let Israel</ns1:w> <ns1:w morph="strongMorph:TH8761" lemma="strong:H03176">hope</ns1:w> <ns1:w lemma="strong:H03068">in the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H05704">from henceforth and for</ns1:w> <ns1:w lemma="strong:H05769">ever</ns1:w>.<ns1:note type="study">henceforth: Heb. now</ns1:note><ns1:verse eID="Ps.131.3" />
