<ns1:verse osisID="Ps.68.5" sID="Ps.68.5" /><ns1:w lemma="strong:H01">A father</ns1:w> <ns1:w lemma="strong:H03490">of the fatherless</ns1:w>, <ns1:w lemma="strong:H01781">and a judge</ns1:w> <ns1:w lemma="strong:H0490">of the widows</ns1:w>, <ns1:transChange type="added">is</ns1:transChange> <ns1:w lemma="strong:H0430">God</ns1:w> <ns1:w lemma="strong:H06944">in his holy</ns1:w> <ns1:w lemma="strong:H04583">habitation</ns1:w>.<ns1:verse eID="Ps.68.5" />
