<ns1:title type="psalm" canonical="true"><ns1:w morph="strongMorph:TH8764" lemma="strong:H05329">To the chief Musician</ns1:w>, <ns1:w lemma="strong:H04210">A Psalm</ns1:w> <ns1:transChange type="added">or</ns1:transChange> <ns1:w lemma="strong:H07892">Song</ns1:w> <ns1:w lemma="strong:H01732">of David</ns1:w>.</ns1:title><ns1:verse osisID="Ps.68.1" sID="Ps.68.1" /><ns1:w lemma="strong:H0430">Let God</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06965">arise</ns1:w>, <ns1:w morph="strongMorph:TH8802" lemma="strong:H0341">let his enemies</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06327">be scattered</ns1:w>: <ns1:w morph="strongMorph:TH8764" lemma="strong:H08130">let them also that hate</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05127">him flee</ns1:w> <ns1:w lemma="strong:H06440">before</ns1:w> him.<ns1:note type="study">before…: Heb. from his face</ns1:note><ns1:verse eID="Ps.68.1" />
<ns1:verse osisID="Ps.68.2" sID="Ps.68.2" /><ns1:w lemma="strong:H06227">As smoke</ns1:w> <ns1:w morph="strongMorph:TH8736" lemma="strong:H05086">is driven away</ns1:w>, <ns1:transChange type="added">so</ns1:transChange> drive <ns1:transChange type="added">them</ns1:transChange> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05086">away</ns1:w>: <ns1:w lemma="strong:H01749">as wax</ns1:w> <ns1:w morph="strongMorph:TH8736" lemma="strong:H04549">melteth</ns1:w> <ns1:w lemma="strong:H06440">before</ns1:w> <ns1:w lemma="strong:H0784">the fire</ns1:w>, <ns1:transChange type="added">so</ns1:transChange> <ns1:w lemma="strong:H07563">let the wicked</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06">perish</ns1:w> <ns1:w lemma="strong:H06440">at the presence</ns1:w> <ns1:w lemma="strong:H0430">of God</ns1:w>.<ns1:verse eID="Ps.68.2" />
<ns1:verse osisID="Ps.68.3" sID="Ps.68.3" /><ns1:w lemma="strong:H06662">But let the righteous</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H08055">be glad</ns1:w>; <ns1:w morph="strongMorph:TH8799" lemma="strong:H05970">let them rejoice</ns1:w> <ns1:w lemma="strong:H06440">before</ns1:w> <ns1:w lemma="strong:H0430">God</ns1:w>: <ns1:w lemma="strong:H08057">yea, let them exceedingly</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H07797">rejoice</ns1:w>.<ns1:note type="study">exceedingly…: Heb. rejoice with gladness</ns1:note><ns1:verse eID="Ps.68.3" />
