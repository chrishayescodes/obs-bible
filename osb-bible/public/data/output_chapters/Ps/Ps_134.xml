<ns1:chapter osisID="Ps.134" chapterTitle="PSALM 134.">
<ns1:title type="chapter">PSALM 134.</ns1:title>
<ns1:title type="psalm" canonical="true"><ns1:w lemma="strong:H07892">A Song</ns1:w> <ns1:w lemma="strong:H04609">of degrees</ns1:w>.</ns1:title><ns1:verse osisID="Ps.134.1" sID="Ps.134.1" /><ns1:w morph="strongMorph:TH8761" lemma="strong:H01288">Behold, bless</ns1:w> <ns1:w lemma="strong:H03068">ye the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>, all <ns1:transChange type="added">ye</ns1:transChange> <ns1:w lemma="strong:H05650">servants</ns1:w> <ns1:w lemma="strong:H03068">of the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>, <ns1:w lemma="strong:H03915">which by night</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H05975">stand</ns1:w> <ns1:w lemma="strong:H01004">in the house</ns1:w> <ns1:w lemma="strong:H03068">of the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>.<ns1:verse eID="Ps.134.1" />
<ns1:verse osisID="Ps.134.2" sID="Ps.134.2" /><ns1:w morph="strongMorph:TH8798" lemma="strong:H05375">Lift up</ns1:w> <ns1:w lemma="strong:H03027">your hands</ns1:w> <ns1:transChange type="added">in</ns1:transChange> <ns1:w lemma="strong:H06944">the sanctuary</ns1:w>, <ns1:w morph="strongMorph:TH8761" lemma="strong:H01288">and bless</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>.<ns1:note type="study">the sanctuary: or, holiness</ns1:note><ns1:verse eID="Ps.134.2" />
<ns1:verse osisID="Ps.134.3" sID="Ps.134.3" /><ns1:w lemma="strong:H03068">The <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H06213">that made</ns1:w> <ns1:w lemma="strong:H08064">heaven</ns1:w> <ns1:w lemma="strong:H0776">and earth</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01288">bless</ns1:w> <ns1:w lemma="strong:H06726">thee out of Zion</ns1:w>.<ns1:verse eID="Ps.134.3" />
