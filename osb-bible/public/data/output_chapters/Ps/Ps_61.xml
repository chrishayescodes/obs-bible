<div type="book" osisID="Ps" canonical="true"><ns1:title type="main">THE BOOK OF PSALMS</ns1:title>
<ns1:chapter osisID="Ps.61" chapterTitle="PSALM 61.">
<ns1:title type="chapter">PSALM 61.</ns1:title>
<ns1:title type="psalm" canonical="true"><ns1:w morph="strongMorph:TH8764" lemma="strong:H05329">To the chief Musician</ns1:w> <ns1:w lemma="strong:H05058">upon Neginah</ns1:w>, <ns1:transChange type="added">A Psalm</ns1:transChange> <ns1:w lemma="strong:H01732">of David</ns1:w>.</ns1:title><ns1:verse osisID="Ps.61.1" sID="Ps.61.1" /><ns1:w morph="strongMorph:TH8798" lemma="strong:H08085">Hear</ns1:w> <ns1:w lemma="strong:H07440">my cry</ns1:w>, <ns1:w lemma="strong:H0430">O God</ns1:w>; <ns1:w morph="strongMorph:TH8685" lemma="strong:H07181">attend</ns1:w> <ns1:w lemma="strong:H08605">unto my prayer</ns1:w>.<ns1:verse eID="Ps.61.1" />
<ns1:verse osisID="Ps.61.2" sID="Ps.61.2" /><ns1:w lemma="strong:H07097">From the end</ns1:w> <ns1:w lemma="strong:H0776">of the earth</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H07121">will I cry</ns1:w> <ns1:w lemma="strong:H03820">unto thee, when my heart</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H05848">is overwhelmed</ns1:w>: <ns1:w morph="strongMorph:TH8686" lemma="strong:H05148">lead</ns1:w> <ns1:w lemma="strong:H06697">me to the rock</ns1:w> <ns1:transChange type="added">that</ns1:transChange> <ns1:w morph="strongMorph:TH8799" lemma="strong:H07311">is higher</ns1:w> than I.<ns1:verse eID="Ps.61.2" />
<ns1:verse osisID="Ps.61.3" sID="Ps.61.3" /><ns1:w lemma="strong:H04268">For thou hast been a shelter</ns1:w> for me, <ns1:transChange type="added">and</ns1:transChange> <ns1:w lemma="strong:H05797">a strong</ns1:w> <ns1:w lemma="strong:H04026">tower</ns1:w> <ns1:w lemma="strong:H06440">from</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H0341">the enemy</ns1:w>.<ns1:verse eID="Ps.61.3" />
<ns1:verse osisID="Ps.61.4" sID="Ps.61.4" /><ns1:w morph="strongMorph:TH8799" lemma="strong:H01481">I will abide</ns1:w> <ns1:w lemma="strong:H0168">in thy tabernacle</ns1:w> <ns1:w lemma="strong:H05769">for ever</ns1:w>: <ns1:w morph="strongMorph:TH8799" lemma="strong:H02620">I will trust</ns1:w> <ns1:w lemma="strong:H05643">in the covert</ns1:w> <ns1:w lemma="strong:H03671">of thy wings</ns1:w>. <ns1:w lemma="strong:H05542">Selah</ns1:w>.<ns1:note type="study">trust: or, make my refuge</ns1:note><ns1:verse eID="Ps.61.4" />
<ns1:verse osisID="Ps.61.5" sID="Ps.61.5" /><ns1:milestone type="x-extra-p" /><ns1:w lemma="strong:H0430">For thou, O God</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H08085">hast heard</ns1:w> <ns1:w lemma="strong:H05088">my vows</ns1:w>: <ns1:w morph="strongMorph:TH8804" lemma="strong:H05414">thou hast given</ns1:w> <ns1:transChange type="added">me</ns1:transChange> <ns1:w lemma="strong:H03425">the heritage</ns1:w> <ns1:w lemma="strong:H03373">of those that fear</ns1:w> <ns1:w lemma="strong:H08034">thy name</ns1:w>.<ns1:verse eID="Ps.61.5" />
<ns1:verse osisID="Ps.61.6" sID="Ps.61.6" /><ns1:w morph="strongMorph:TH8686" lemma="strong:H03254">Thou wilt prolong</ns1:w> <ns1:w lemma="strong:H04428">the king's</ns1:w> <ns1:w lemma="strong:H03117 strong:H03117">life</ns1:w>: <ns1:transChange type="added">and</ns1:transChange> <ns1:w lemma="strong:H08141">his years</ns1:w> <ns1:w lemma="strong:H01755">as many</ns1:w> <ns1:w lemma="strong:H01755">generations</ns1:w>.<ns1:note type="study">Thou…: Heb. Thou shalt add days to the days of the king</ns1:note><ns1:note type="study">as…: Heb. as generation and generation</ns1:note><ns1:verse eID="Ps.61.6" />
<ns1:verse osisID="Ps.61.7" sID="Ps.61.7" /><ns1:w morph="strongMorph:TH8799" lemma="strong:H03427">He shall abide</ns1:w> <ns1:w lemma="strong:H06440">before</ns1:w> <ns1:w lemma="strong:H0430">God</ns1:w> <ns1:w lemma="strong:H05769">for ever</ns1:w>: <ns1:w morph="strongMorph:TH8761" lemma="strong:H04487">O prepare</ns1:w> <ns1:w lemma="strong:H02617">mercy</ns1:w> <ns1:w lemma="strong:H0571">and truth</ns1:w>, <ns1:transChange type="added">which</ns1:transChange> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05341">may preserve</ns1:w> him.<ns1:verse eID="Ps.61.7" />
<ns1:verse osisID="Ps.61.8" sID="Ps.61.8" /><ns1:w morph="strongMorph:TH8762" lemma="strong:H02167">So will I sing</ns1:w> <ns1:w lemma="strong:H08034">praise unto thy name</ns1:w> <ns1:w lemma="strong:H05703">for ever</ns1:w>, <ns1:w lemma="strong:H03117 strong:H03117">that I may daily</ns1:w> <ns1:w morph="strongMorph:TH8763" lemma="strong:H07999">perform</ns1:w> <ns1:w lemma="strong:H05088">my vows</ns1:w>.<ns1:verse eID="Ps.61.8" />
</ns1:chapter>
