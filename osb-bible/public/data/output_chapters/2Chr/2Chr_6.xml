<ns1:verse osisID="2Chr.6.4" sID="2Chr.6.4" /><ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">And he said</ns1:w>, <ns1:w morph="strongMorph:TH8803" lemma="strong:H01288">Blessed</ns1:w> <ns1:transChange type="added">be</ns1:transChange> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H0430">God</ns1:w> <ns1:w lemma="strong:H03478">of Israel</ns1:w>, <ns1:w lemma="strong:H03027">who hath with his hands</ns1:w> <ns1:w morph="strongMorph:TH8765" lemma="strong:H04390">fulfilled</ns1:w> <ns1:transChange type="added">that</ns1:transChange> <ns1:w morph="strongMorph:TH8765" lemma="strong:H01696">which he spake</ns1:w> <ns1:w lemma="strong:H06310">with his mouth</ns1:w> <ns1:w lemma="strong:H01">to my father</ns1:w> <ns1:w lemma="strong:H01732">David</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>,<ns1:verse eID="2Chr.6.4" />
