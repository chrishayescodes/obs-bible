<ns1:verse osisID="Neh.11.35" sID="Neh.11.35" /><ns1:w lemma="strong:H03850">Lod</ns1:w>, <ns1:w lemma="strong:H0207">and Ono</ns1:w>, <ns1:w lemma="strong:H01516">the valley</ns1:w> <ns1:w morph="strongMorph:TH8677" lemma="strong:H02798 strong:H02791">of craftsmen</ns1:w>.<ns1:verse eID="Neh.11.35" />
<ns1:verse osisID="Neh.11.36" sID="Neh.11.36" /><ns1:w lemma="strong:H03881">And of the Levites</ns1:w> <ns1:transChange type="added">were</ns1:transChange> <ns1:w lemma="strong:H04256">divisions</ns1:w> <ns1:transChange type="added">in</ns1:transChange> <ns1:w lemma="strong:H03063">Judah</ns1:w>, <ns1:transChange type="added">and</ns1:transChange> <ns1:w lemma="strong:H01144">in Benjamin</ns1:w>.<ns1:verse eID="Neh.11.36" />
