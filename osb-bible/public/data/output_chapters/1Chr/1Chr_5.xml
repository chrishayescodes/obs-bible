<ns1:verse osisID="1Chr.5.26" sID="1Chr.5.26" /><ns1:w lemma="strong:H0430">And the God</ns1:w> <ns1:w lemma="strong:H03478">of Israel</ns1:w> <ns1:w morph="strongMorph:TH8686" lemma="strong:H05782">stirred up</ns1:w> <ns1:w lemma="strong:H07307">the spirit</ns1:w> <ns1:w lemma="strong:H06322">of Pul</ns1:w> <ns1:w lemma="strong:H04428">king</ns1:w> <ns1:w lemma="strong:H0804">of Assyria</ns1:w>, <ns1:w lemma="strong:H07307">and the spirit</ns1:w> <ns1:w lemma="strong:H08407">of Tilgath–pilneser</ns1:w> <ns1:w lemma="strong:H04428">king</ns1:w> <ns1:w lemma="strong:H0804">of Assyria</ns1:w>, <ns1:w morph="strongMorph:TH8686" lemma="strong:H01540">and he carried them away</ns1:w>, <ns1:w lemma="strong:H07206">even the Reubenites</ns1:w>, <ns1:w lemma="strong:H01425">and the Gadites</ns1:w>, <ns1:w lemma="strong:H02677">and the half</ns1:w> <ns1:w lemma="strong:H07626">tribe</ns1:w> <ns1:w lemma="strong:H04519">of Manasseh</ns1:w>, <ns1:w morph="strongMorph:TH8686" lemma="strong:H0935">and brought</ns1:w> <ns1:w lemma="strong:H02477">them unto Halah</ns1:w>, <ns1:w lemma="strong:H02249">and Habor</ns1:w>, <ns1:w lemma="strong:H02024">and Hara</ns1:w>, <ns1:w lemma="strong:H05104">and to the river</ns1:w> <ns1:w lemma="strong:H01470">Gozan</ns1:w>, <ns1:w lemma="strong:H03117">unto this day</ns1:w>.<ns1:verse eID="1Chr.5.26" />
