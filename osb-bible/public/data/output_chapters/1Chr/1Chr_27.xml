<ns1:title type="chapter">CHAPTER 27.</ns1:title>
<ns1:verse osisID="1Chr.27.1" sID="1Chr.27.1" /><ns1:w lemma="strong:H01121">Now the children</ns1:w> <ns1:w lemma="strong:H03478">of Israel</ns1:w> <ns1:w lemma="strong:H04557">after their number</ns1:w>, <ns1:transChange type="added">to wit</ns1:transChange>, <ns1:w lemma="strong:H07218">the chief</ns1:w> <ns1:w lemma="strong:H01">fathers</ns1:w> <ns1:w lemma="strong:H08269">and captains</ns1:w> <ns1:w lemma="strong:H0505">of thousands</ns1:w> <ns1:w lemma="strong:H03967">and hundreds</ns1:w>, <ns1:w morph="strongMorph:TH8802" lemma="strong:H07860">and their officers</ns1:w> <ns1:w morph="strongMorph:TH8764" lemma="strong:H08334">that served</ns1:w> <ns1:w lemma="strong:H04428">the king</ns1:w> <ns1:w lemma="strong:H01697">in any matter</ns1:w> <ns1:w lemma="strong:H04256">of the courses</ns1:w>, <ns1:w morph="strongMorph:TH8802" lemma="strong:H0935">which came in</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H03318">and went out</ns1:w> <ns1:w lemma="strong:H02320">month</ns1:w> <ns1:w lemma="strong:H02320">by month</ns1:w> <ns1:w lemma="strong:H02320">throughout all the months</ns1:w> <ns1:w lemma="strong:H08141">of the year</ns1:w>, <ns1:w lemma="strong:H0259">of every</ns1:w> <ns1:w lemma="strong:H04256">course</ns1:w> <ns1:transChange type="added">were</ns1:transChange> <ns1:w lemma="strong:H06242">twenty</ns1:w> <ns1:w lemma="strong:H0702">and four</ns1:w> <ns1:w lemma="strong:H0505">thousand</ns1:w>.<ns1:verse eID="1Chr.27.1" />
<ns1:verse osisID="1Chr.27.2" sID="1Chr.27.2" /><ns1:w lemma="strong:H07223">Over the first</ns1:w> <ns1:w lemma="strong:H04256">course</ns1:w> <ns1:w lemma="strong:H07223">for the first</ns1:w> <ns1:w lemma="strong:H02320">month</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H03434">Jashobeam</ns1:w> <ns1:w lemma="strong:H01121">the son</ns1:w> <ns1:w lemma="strong:H02068">of Zabdiel</ns1:w>: <ns1:w lemma="strong:H04256">and in his course</ns1:w> <ns1:transChange type="added">were</ns1:transChange> <ns1:w lemma="strong:H06242">twenty</ns1:w> <ns1:w lemma="strong:H0702">and four</ns1:w> <ns1:w lemma="strong:H0505">thousand</ns1:w>.<ns1:verse eID="1Chr.27.2" />
