<ns1:verse osisID="2Kgs.16.1" sID="2Kgs.16.1" /><ns1:w lemma="strong:H08141 strong:H06240 strong:H07651">In the seventeenth</ns1:w> <ns1:w lemma="strong:H08141">year</ns1:w> <ns1:w lemma="strong:H06492">of Pekah</ns1:w> <ns1:w lemma="strong:H01121">the son</ns1:w> <ns1:w lemma="strong:H07425">of Remaliah</ns1:w> <ns1:w lemma="strong:H0271">Ahaz</ns1:w> <ns1:w lemma="strong:H01121">the son</ns1:w> <ns1:w lemma="strong:H03147">of Jotham</ns1:w> <ns1:w lemma="strong:H04428">king</ns1:w> <ns1:w lemma="strong:H03063">of Judah</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H04427">began to reign</ns1:w>.<ns1:verse eID="2Kgs.16.1" />
