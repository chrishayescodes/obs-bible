<ns1:verse osisID="Gen.19.4" sID="Gen.19.4" /><ns1:milestone type="x-p" marker="¶" /><ns1:w morph="strongMorph:TH8799" lemma="strong:H07901">But before they lay down</ns1:w>, <ns1:w lemma="strong:H0582">the men</ns1:w> <ns1:w lemma="strong:H05892">of the city</ns1:w>, <ns1:transChange type="added">even</ns1:transChange> <ns1:w lemma="strong:H0582">the men</ns1:w> <ns1:w lemma="strong:H05467">of Sodom</ns1:w>, <ns1:w lemma="strong:H05437">compassed</ns1:w> <ns1:w lemma="strong:H01004">the house</ns1:w> <ns1:w morph="strongMorph:TH8738" lemma="strong:H05437">round</ns1:w>, <ns1:w lemma="strong:H02205">both old</ns1:w> <ns1:w lemma="strong:H05288">and young</ns1:w>, <ns1:w lemma="strong:H05971">all the people</ns1:w> <ns1:w lemma="strong:H07097">from every quarter</ns1:w>:<ns1:verse eID="Gen.19.4" />
