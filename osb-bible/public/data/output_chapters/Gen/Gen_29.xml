<ns1:verse osisID="Gen.29.15" sID="Gen.29.15" /><ns1:milestone type="x-p" marker="¶" /><ns1:w lemma="strong:H03837">And Laban</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w> <ns1:w lemma="strong:H03290">unto Jacob</ns1:w>, Because thou <ns1:transChange type="added">art</ns1:transChange> <ns1:w lemma="strong:H0251">my brother</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H05647">shouldest thou therefore serve</ns1:w> <ns1:w lemma="strong:H02600">me for nought</ns1:w>? <ns1:w morph="strongMorph:TH8685" lemma="strong:H05046">tell</ns1:w> me, what <ns1:transChange type="added">shall</ns1:transChange> <ns1:w lemma="strong:H04909">thy wages</ns1:w> <ns1:transChange type="added">be</ns1:transChange>?<ns1:verse eID="Gen.29.15" />
<ns1:verse osisID="Gen.29.16" sID="Gen.29.16" /><ns1:w lemma="strong:H03837">And Laban</ns1:w> <ns1:w lemma="strong:H08147">had two</ns1:w> <ns1:w lemma="strong:H01323">daughters</ns1:w>: <ns1:w lemma="strong:H08034">the name</ns1:w> <ns1:w lemma="strong:H01419">of the elder</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H03812">Leah</ns1:w>, <ns1:w lemma="strong:H08034">and the name</ns1:w> <ns1:w lemma="strong:H06996">of the younger</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H07354">Rachel</ns1:w>.<ns1:verse eID="Gen.29.16" />
<ns1:verse osisID="Gen.29.17" sID="Gen.29.17" /><ns1:w lemma="strong:H03812">Leah</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H07390">tender</ns1:w> <ns1:w lemma="strong:H05869">eyed</ns1:w>; <ns1:w lemma="strong:H07354">but Rachel</ns1:w> <ns1:w lemma="strong:H08389 strong:H03303">was beautiful</ns1:w> <ns1:w lemma="strong:H03303">and well</ns1:w> <ns1:w lemma="strong:H04758">favoured</ns1:w>.<ns1:verse eID="Gen.29.17" />
<ns1:verse osisID="Gen.29.18" sID="Gen.29.18" /><ns1:w lemma="strong:H03290">And Jacob</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0157">loved</ns1:w> <ns1:w lemma="strong:H07354">Rachel</ns1:w>; <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">and said</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H05647">I will serve</ns1:w> <ns1:w lemma="strong:H07651">thee seven</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w> <ns1:w lemma="strong:H07354">for Rachel</ns1:w> <ns1:w lemma="strong:H06996">thy younger</ns1:w> <ns1:w lemma="strong:H01323">daughter</ns1:w>.<ns1:verse eID="Gen.29.18" />
<ns1:verse osisID="Gen.29.19" sID="Gen.29.19" /><ns1:w lemma="strong:H03837">And Laban</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w>, <ns1:transChange type="added">It is</ns1:transChange> <ns1:w lemma="strong:H02896">better</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H05414">that I give</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H05414">her to thee, than that I should give</ns1:w> <ns1:w lemma="strong:H0312">her to another</ns1:w> <ns1:w lemma="strong:H0376">man</ns1:w>: <ns1:w morph="strongMorph:TH8798" lemma="strong:H03427">abide</ns1:w> with me.<ns1:verse eID="Gen.29.19" />
<ns1:verse osisID="Gen.29.20" sID="Gen.29.20" /><ns1:w lemma="strong:H03290">And Jacob</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05647">served</ns1:w> <ns1:w lemma="strong:H07651">seven</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w> <ns1:w lemma="strong:H07354">for Rachel</ns1:w>; <ns1:w lemma="strong:H05869">and they seemed</ns1:w> unto him <ns1:transChange type="added">but</ns1:transChange> <ns1:w lemma="strong:H0259">a few</ns1:w> <ns1:w lemma="strong:H03117">days</ns1:w>, <ns1:w lemma="strong:H0160">for the love he had</ns1:w> to her.<ns1:verse eID="Gen.29.20" />
<ns1:verse osisID="Gen.29.21" sID="Gen.29.21" /><ns1:milestone type="x-p" subType="x-added" marker="¶" /><ns1:w lemma="strong:H03290">And Jacob</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w> <ns1:w lemma="strong:H03837">unto Laban</ns1:w>, <ns1:w morph="strongMorph:TH8798" lemma="strong:H03051">Give</ns1:w> <ns1:transChange type="added">me</ns1:transChange> <ns1:w lemma="strong:H0802">my wife</ns1:w>, <ns1:w lemma="strong:H03117">for my days</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H04390">are fulfilled</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H0935">that I may go in</ns1:w> unto her.<ns1:verse eID="Gen.29.21" />
