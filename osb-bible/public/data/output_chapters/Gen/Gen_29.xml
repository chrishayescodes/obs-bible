<ns1:verse osisID="Gen.29.30" sID="Gen.29.30" /><ns1:w morph="strongMorph:TH8799" lemma="strong:H0935">And he went in</ns1:w> <ns1:w lemma="strong:H07354">also unto Rachel</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H0157">and he loved</ns1:w> <ns1:w lemma="strong:H07354">also Rachel</ns1:w> <ns1:w lemma="strong:H03812">more than Leah</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H05647">and served</ns1:w> <ns1:w lemma="strong:H07651">with him yet seven</ns1:w> <ns1:w lemma="strong:H0312">other</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w>.<ns1:verse eID="Gen.29.30" />
<ns1:verse osisID="Gen.29.31" sID="Gen.29.31" /><ns1:milestone type="x-p" marker="¶" /><ns1:w lemma="strong:H03068">And when the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H07200">saw</ns1:w> <ns1:w lemma="strong:H03812">that Leah</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w morph="strongMorph:TH8803" lemma="strong:H08130">hated</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H06605">he opened</ns1:w> <ns1:w lemma="strong:H07358">her womb</ns1:w>: <ns1:w lemma="strong:H07354">but Rachel</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H06135">barren</ns1:w>.<ns1:verse eID="Gen.29.31" />
<ns1:verse osisID="Gen.29.32" sID="Gen.29.32" /><ns1:w lemma="strong:H03812">And Leah</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H02029">conceived</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H03205">and bare</ns1:w> <ns1:w lemma="strong:H01121">a son</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H07121">and she called</ns1:w> <ns1:w lemma="strong:H08034">his name</ns1:w> <ns1:w lemma="strong:H07205">Reuben</ns1:w>: <ns1:w lemma="strong:H03588">for</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H0559">she said</ns1:w>, <ns1:w lemma="strong:H03588">Surely</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H07200">hath looked</ns1:w> <ns1:w lemma="strong:H06040">upon my affliction</ns1:w>; <ns1:w lemma="strong:H03588">now therefore</ns1:w> <ns1:w lemma="strong:H0376">my husband</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0157">will love</ns1:w> me.<ns1:note type="study">Reuben: that is, See a son</ns1:note><ns1:verse eID="Gen.29.32" />
