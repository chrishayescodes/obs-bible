<ns1:verse osisID="Gen.29.24" sID="Gen.29.24" /><ns1:w lemma="strong:H03837">And Laban</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05414">gave</ns1:w> <ns1:w lemma="strong:H01323">unto his daughter</ns1:w> <ns1:w lemma="strong:H03812">Leah</ns1:w> <ns1:w lemma="strong:H02153">Zilpah</ns1:w> <ns1:w lemma="strong:H08198">his maid</ns1:w> <ns1:transChange type="added">for</ns1:transChange> <ns1:w lemma="strong:H08198">an handmaid</ns1:w>.<ns1:verse eID="Gen.29.24" />
<ns1:verse osisID="Gen.29.25" sID="Gen.29.25" /><ns1:w lemma="strong:H01242">And it came to pass, that in the morning</ns1:w>, behold, it <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H03812">Leah</ns1:w>: <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">and he said</ns1:w> <ns1:w lemma="strong:H03837">to Laban</ns1:w>, What <ns1:transChange type="added">is</ns1:transChange> <ns1:w morph="strongMorph:TH8804" lemma="strong:H06213">this thou hast done</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H05647">unto me? did not I serve</ns1:w> <ns1:w lemma="strong:H07354">with thee for Rachel</ns1:w>? <ns1:w morph="strongMorph:TH8765" lemma="strong:H07411">wherefore then hast thou beguiled</ns1:w> me?<ns1:verse eID="Gen.29.25" />
<ns1:verse osisID="Gen.29.26" sID="Gen.29.26" /><ns1:w lemma="strong:H03837">And Laban</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w>, <ns1:w morph="strongMorph:TH8735" lemma="strong:H06213">It must not be so done</ns1:w> <ns1:w lemma="strong:H04725">in our country</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H05414">to give</ns1:w> <ns1:w lemma="strong:H06810">the younger</ns1:w> <ns1:w lemma="strong:H06440">before</ns1:w> <ns1:w lemma="strong:H01067">the firstborn</ns1:w>.<ns1:note type="study">country: Heb. place</ns1:note><ns1:verse eID="Gen.29.26" />
<ns1:verse osisID="Gen.29.27" sID="Gen.29.27" /><ns1:w morph="strongMorph:TH8761" lemma="strong:H04390">Fulfil</ns1:w> <ns1:w lemma="strong:H02063">her</ns1:w> <ns1:w lemma="strong:H07620">week</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H05414">and we will give</ns1:w> <ns1:w lemma="strong:H05656">thee this also for the service</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05647">which thou shalt serve</ns1:w> <ns1:w lemma="strong:H07651">with me yet seven</ns1:w> <ns1:w lemma="strong:H0312">other</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w>.<ns1:verse eID="Gen.29.27" />
