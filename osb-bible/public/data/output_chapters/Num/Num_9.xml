<ns1:verse osisID="Num.9.4" sID="Num.9.4" /><ns1:w lemma="strong:H04872">And Moses</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01696">spake</ns1:w> <ns1:w lemma="strong:H01121">unto the children</ns1:w> <ns1:w lemma="strong:H03478">of Israel</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H06213">that they should keep</ns1:w> <ns1:w lemma="strong:H06453">the passover</ns1:w>.<ns1:verse eID="Num.9.4" />
