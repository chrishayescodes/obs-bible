<ns1:verse osisID="Num.16.4" sID="Num.16.4" /><ns1:w lemma="strong:H04872">And when Moses</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H08085">heard</ns1:w> <ns1:transChange type="added">it</ns1:transChange>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H05307">he fell</ns1:w> <ns1:w lemma="strong:H06440">upon his face</ns1:w>:<ns1:verse eID="Num.16.4" />
