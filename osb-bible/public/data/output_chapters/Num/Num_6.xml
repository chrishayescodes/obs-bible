<ns1:verse osisID="Num.6.22" sID="Num.6.22" /><ns1:milestone type="x-p" marker="¶" /><ns1:w lemma="strong:H03068">And the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01696">spake</ns1:w> <ns1:w lemma="strong:H04872">unto Moses</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>,<ns1:verse eID="Num.6.22" />
<ns1:verse osisID="Num.6.23" sID="Num.6.23" /><ns1:w morph="strongMorph:TH8761" lemma="strong:H01696">Speak</ns1:w> <ns1:w lemma="strong:H0175">unto Aaron</ns1:w> <ns1:w lemma="strong:H01121">and unto his sons</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>, <ns1:w lemma="strong:H03541">On this wise</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01288">ye shall bless</ns1:w> <ns1:w lemma="strong:H01121">the children</ns1:w> <ns1:w lemma="strong:H03478">of Israel</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w> unto them,<ns1:verse eID="Num.6.23" />
<ns1:verse osisID="Num.6.24" sID="Num.6.24" /><ns1:w lemma="strong:H03068">The <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01288">bless</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H08104">thee, and keep</ns1:w> thee:<ns1:verse eID="Num.6.24" />
<ns1:verse osisID="Num.6.25" sID="Num.6.25" /><ns1:w lemma="strong:H03068">The <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H06440">make his face</ns1:w> <ns1:w morph="strongMorph:TH8686" lemma="strong:H0215">shine</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H02603">upon thee, and be gracious</ns1:w> unto thee:<ns1:verse eID="Num.6.25" />
<ns1:verse osisID="Num.6.26" sID="Num.6.26" /><ns1:w lemma="strong:H03068">The <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05375">lift up</ns1:w> <ns1:w lemma="strong:H06440">his countenance</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H07760">upon thee, and give</ns1:w> <ns1:w lemma="strong:H07965">thee peace</ns1:w>.<ns1:verse eID="Num.6.26" />
<ns1:verse osisID="Num.6.27" sID="Num.6.27" /><ns1:w morph="strongMorph:TH8804" lemma="strong:H07760">And they shall put</ns1:w> <ns1:w lemma="strong:H08034">my name</ns1:w> <ns1:w lemma="strong:H01121">upon the children</ns1:w> <ns1:w lemma="strong:H03478">of Israel</ns1:w>; <ns1:w morph="strongMorph:TH8762" lemma="strong:H01288">and I will bless</ns1:w> them.<ns1:verse eID="Num.6.27" />
