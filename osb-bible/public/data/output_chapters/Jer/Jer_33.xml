<ns1:verse osisID="Jer.33.14" sID="Jer.33.14" /><ns1:w lemma="strong:H03117">Behold, the days</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H0935">come</ns1:w>, <ns1:w morph="strongMorph:TH8803" lemma="strong:H05002">saith</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>, <ns1:w morph="strongMorph:TH8689" lemma="strong:H06965">that I will perform</ns1:w> <ns1:w lemma="strong:H02896">that good</ns1:w> <ns1:w lemma="strong:H01697">thing</ns1:w> <ns1:w morph="strongMorph:TH8765" lemma="strong:H01696">which I have promised</ns1:w> <ns1:w lemma="strong:H01004">unto the house</ns1:w> <ns1:w lemma="strong:H03478">of Israel</ns1:w> <ns1:w lemma="strong:H01004">and to the house</ns1:w> <ns1:w lemma="strong:H03063">of Judah</ns1:w>.<ns1:verse eID="Jer.33.14" />
