<ns1:verse osisID="Jer.33.18" sID="Jer.33.18" /><ns1:w lemma="strong:H03548">Neither shall the priests</ns1:w> <ns1:w lemma="strong:H03881">the Levites</ns1:w> <ns1:w morph="strongMorph:TH8735" lemma="strong:H03772">want</ns1:w> <ns1:w lemma="strong:H0376">a man</ns1:w> <ns1:w lemma="strong:H06440">before</ns1:w> <ns1:w morph="strongMorph:TH8688" lemma="strong:H05927">me to offer</ns1:w> <ns1:w lemma="strong:H05930">burnt offerings</ns1:w>, <ns1:w morph="strongMorph:TH8688" lemma="strong:H06999">and to kindle</ns1:w> <ns1:w lemma="strong:H04503">meat offerings</ns1:w>, <ns1:w morph="strongMorph:TH8802" lemma="strong:H06213">and to do</ns1:w> <ns1:w lemma="strong:H02077">sacrifice</ns1:w> <ns1:w lemma="strong:H03117">continually</ns1:w>.<ns1:verse eID="Jer.33.18" />
