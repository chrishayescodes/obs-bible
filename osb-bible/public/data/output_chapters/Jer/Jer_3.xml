<ns1:verse osisID="Jer.3.13" sID="Jer.3.13" /><ns1:w morph="strongMorph:TH8798" lemma="strong:H03045">Only acknowledge</ns1:w> <ns1:w lemma="strong:H05771">thine iniquity</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H06586">that thou hast transgressed</ns1:w> <ns1:w lemma="strong:H03068">against the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H0430">thy God</ns1:w>, <ns1:w morph="strongMorph:TH8762" lemma="strong:H06340">and hast scattered</ns1:w> <ns1:w lemma="strong:H01870">thy ways</ns1:w> <ns1:w morph="strongMorph:TH8801" lemma="strong:H02114">to the strangers</ns1:w> <ns1:w lemma="strong:H07488">under every green</ns1:w> <ns1:w lemma="strong:H06086">tree</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H08085">and ye have not obeyed</ns1:w> <ns1:w lemma="strong:H06963">my voice</ns1:w>, <ns1:w morph="strongMorph:TH8803" lemma="strong:H05002">saith</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>.<ns1:verse eID="Jer.3.13" />
<ns1:verse osisID="Jer.3.14" sID="Jer.3.14" /><ns1:w morph="strongMorph:TH8798" lemma="strong:H07725">Turn</ns1:w>, <ns1:w lemma="strong:H07726">O backsliding</ns1:w> <ns1:w lemma="strong:H01121">children</ns1:w>, <ns1:w morph="strongMorph:TH8803" lemma="strong:H05002">saith</ns1:w> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>; <ns1:w morph="strongMorph:TH8804" lemma="strong:H01166">for I am married</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H03947">unto you: and I will take</ns1:w> <ns1:w lemma="strong:H0259">you one</ns1:w> <ns1:w lemma="strong:H05892">of a city</ns1:w>, <ns1:w lemma="strong:H08147">and two</ns1:w> <ns1:w lemma="strong:H04940">of a family</ns1:w>, <ns1:w morph="strongMorph:TH8689" lemma="strong:H0935">and I will bring</ns1:w> <ns1:w lemma="strong:H06726">you to Zion</ns1:w>:<ns1:verse eID="Jer.3.14" />
