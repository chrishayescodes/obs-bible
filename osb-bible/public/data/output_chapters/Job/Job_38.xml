<ns1:verse osisID="Job.38.1" sID="Job.38.1" /><ns1:w lemma="strong:H03068">Then the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06030">answered</ns1:w> <ns1:w lemma="strong:H0347">Job</ns1:w> <ns1:w lemma="strong:H05591">out of the whirlwind</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">and said</ns1:w>,<ns1:verse eID="Job.38.1" />
