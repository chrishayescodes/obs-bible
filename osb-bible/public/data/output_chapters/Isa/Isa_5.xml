<ns1:verse osisID="Isa.5.10" sID="Isa.5.10" /><ns1:w lemma="strong:H03588">Yea</ns1:w>, <ns1:w lemma="strong:H06235">ten</ns1:w> <ns1:w lemma="strong:H06776">acres</ns1:w> <ns1:w lemma="strong:H03754">of vineyard</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06213">shall yield</ns1:w> <ns1:w lemma="strong:H0259">one</ns1:w> <ns1:w lemma="strong:H01324">bath</ns1:w>, <ns1:w lemma="strong:H02233">and the seed</ns1:w> <ns1:w lemma="strong:H02563">of an homer</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06213">shall yield</ns1:w> <ns1:w lemma="strong:H0374">an ephah</ns1:w>.<ns1:verse eID="Isa.5.10" />
