<ns1:verse osisID="Isa.22.15" sID="Isa.22.15" /><ns1:milestone type="x-p" marker="¶" /><ns1:w morph="strongMorph:TH8804" lemma="strong:H0559">Thus saith</ns1:w> <ns1:w lemma="strong:H0136">the Lord</ns1:w> <ns1:w lemma="strong:H03069"><ns1:seg><ns1:divineName>God</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H06635">of hosts</ns1:w>, <ns1:w morph="strongMorph:TH8798" lemma="strong:H03212">Go</ns1:w>, <ns1:w morph="strongMorph:TH8798" lemma="strong:H0935">get</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H05532">thee unto this treasurer</ns1:w>, <ns1:transChange type="added">even</ns1:transChange> <ns1:w lemma="strong:H07644">unto Shebna</ns1:w>, which <ns1:transChange type="added">is</ns1:transChange> <ns1:w lemma="strong:H01004">over the house</ns1:w>, <ns1:transChange type="added">and say</ns1:transChange>,<ns1:verse eID="Isa.22.15" />
