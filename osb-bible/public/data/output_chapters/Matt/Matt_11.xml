<ns1:title type="chapter">CHAPTER 11.</ns1:title>
<ns1:verse osisID="Matt.11.1" sID="Matt.11.1" /><ns1:w src="1" lemma="strong:G2532" morph="robinson:CONJ">And</ns1:w> <ns1:w src="2" lemma="strong:G1096" morph="robinson:V-2ADI-3S">it came to pass</ns1:w>, <ns1:w src="3" lemma="strong:G3753" morph="robinson:ADV">when</ns1:w> <ns1:w src="5 6" lemma="strong:G3588 strong:G2424" morph="robinson:T-NSM robinson:N-NSM">Jesus</ns1:w> <ns1:w src="4" lemma="strong:G5055" morph="robinson:V-AAI-3S">had made an end of</ns1:w> <ns1:w src="7" lemma="strong:G1299" morph="robinson:V-PAP-NSM">commanding</ns1:w> <ns1:w src="11" lemma="strong:G846" morph="robinson:P-GSM">his</ns1:w> <ns1:w src="9" lemma="strong:G1427" morph="robinson:A-NUI">twelve</ns1:w> <ns1:w src="8" lemma="strong:G3588" morph="robinson:T-DPM" /><ns1:w src="10" lemma="strong:G3101" morph="robinson:N-DPM">disciples</ns1:w>, <ns1:w src="12" lemma="strong:G3327" morph="robinson:V-2AAI-3S">he departed</ns1:w> <ns1:w src="13" lemma="strong:G1564" morph="robinson:ADV">thence</ns1:w> <ns1:w src="14" lemma="strong:G3588" morph="robinson:T-GSM" /><ns1:w src="15" lemma="strong:G1321" morph="robinson:V-PAN">to teach</ns1:w> <ns1:w src="16" lemma="strong:G2532" morph="robinson:CONJ">and</ns1:w> <ns1:w src="17" lemma="strong:G2784" morph="robinson:V-PAN">to preach</ns1:w> <ns1:w src="18" lemma="strong:G1722" morph="robinson:PREP">in</ns1:w> <ns1:w src="21" lemma="strong:G846" morph="robinson:P-GPM">their</ns1:w> <ns1:w src="19 20" lemma="strong:G3588 strong:G4172" morph="robinson:T-DPF robinson:N-DPF">cities</ns1:w>.<ns1:milestone type="x-strongsMarkup" resp="ank 2003-11-30-20:10" /><ns1:verse eID="Matt.11.1" />
<ns1:verse osisID="Matt.11.2" sID="Matt.11.2" /><ns1:w src="2" lemma="strong:G1161" morph="robinson:CONJ">Now when</ns1:w> <ns1:w src="1" lemma="strong:G3588" morph="robinson:T-NSM" /><ns1:w src="3" lemma="strong:G2491" morph="robinson:N-NSM">John</ns1:w> <ns1:w src="4" lemma="strong:G191" morph="robinson:V-AAP-NSM">had heard</ns1:w> <ns1:w src="5" lemma="strong:G1722" morph="robinson:PREP">in</ns1:w> <ns1:w src="6 7" lemma="strong:G3588 strong:G1201" morph="robinson:T-DSN robinson:N-DSN">the prison</ns1:w> <ns1:w src="8 9" lemma="strong:G3588 strong:G2041" morph="robinson:T-APN robinson:N-APN">the works</ns1:w> <ns1:w src="10 11" lemma="strong:G3588 strong:G5547" morph="robinson:T-GSM robinson:N-GSM">of Christ</ns1:w>, <ns1:w src="12" lemma="strong:G3992" morph="robinson:V-AAP-NSM">he sent</ns1:w> <ns1:w src="13" lemma="strong:G1417" morph="robinson:A-NUI">two</ns1:w> <ns1:w src="14 15" lemma="strong:G3588 strong:G3101" morph="robinson:T-GPM robinson:N-GPM">of</ns1:w> <ns1:w src="16" lemma="strong:G846" morph="robinson:P-GSM">his</ns1:w> <ns1:w src="15" lemma="strong:G3101" morph="robinson:N-GPM" type="x-split" subType="x-17">disciples</ns1:w>,<ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-10-11:46" /><ns1:verse eID="Matt.11.2" />
<ns1:verse osisID="Matt.11.3" sID="Matt.11.3" /><ns1:w src="1" lemma="strong:G2036" morph="robinson:V-2AAI-3S">And said</ns1:w> <ns1:w src="2" lemma="strong:G846" morph="robinson:P-DSM">unto him</ns1:w>, <ns1:w src="4" lemma="strong:G1488" morph="robinson:V-PXI-2S">Art</ns1:w> <ns1:w src="3" lemma="strong:G4771" morph="robinson:P-2NS">thou</ns1:w> <ns1:w src="5" lemma="strong:G3588" morph="robinson:T-NSM" /><ns1:w src="6" lemma="strong:G2064" morph="robinson:V-PNP-NSM">he that should come</ns1:w>, <ns1:w src="7" lemma="strong:G2228" morph="robinson:PRT">or</ns1:w> <ns1:w src="9" lemma="strong:G4328" morph="robinson:V-PAI-1P">do we look for</ns1:w> <ns1:w src="8" lemma="strong:G2087" morph="robinson:A-ASM">another</ns1:w>?<ns1:note type="x-strongsMarkup" resp="ank 2003-12-10-11:47">eipen -- "and said" doesn't feel right.</ns1:note><ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-10-11:47" /><ns1:verse eID="Matt.11.3" />
<ns1:verse osisID="Matt.11.4" sID="Matt.11.4" /><ns1:w src="1" lemma="strong:G2532" morph="robinson:CONJ" /><ns1:w src="3 4" lemma="strong:G3588 strong:G2424" morph="robinson:T-NSM robinson:N-NSM">Jesus</ns1:w> <ns1:w src="2" lemma="strong:G611" morph="robinson:V-AOP-NSM">answered</ns1:w> <ns1:w src="5" lemma="strong:G2036" morph="robinson:V-2AAI-3S">and said</ns1:w> <ns1:w src="6" lemma="strong:G846" morph="robinson:P-DPM">unto them</ns1:w>, <ns1:q who="Jesus" marker=""><ns1:w src="7" lemma="strong:G4198" morph="robinson:V-AOP-NPM">Go</ns1:w> <ns1:w src="8" lemma="strong:G518" morph="robinson:V-AAM-2P">and shew</ns1:w> <ns1:w src="9" lemma="strong:G2491" morph="robinson:N-DSM">John</ns1:w> <ns1:w src="8" lemma="strong:G518" morph="robinson:V-AAM-2P" type="x-split" subType="x-14">again</ns1:w> <ns1:w src="10" lemma="strong:G3739" morph="robinson:R-APN">those things which</ns1:w> <ns1:w src="11" lemma="strong:G191" morph="robinson:V-PAI-2P">ye do hear</ns1:w> <ns1:w src="12" lemma="strong:G2532" morph="robinson:CONJ">and</ns1:w> <ns1:w src="13" lemma="strong:G991" morph="robinson:V-PAI-2P">see</ns1:w>:<ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-02-06:12" /><ns1:verse eID="Matt.11.4" />
<ns1:verse osisID="Matt.11.5" sID="Matt.11.5" /><ns1:w src="1" lemma="strong:G5185" morph="robinson:A-NPM">The blind</ns1:w> <ns1:w src="2" lemma="strong:G308" morph="robinson:V-PAI-3P">receive their sight</ns1:w>, <ns1:w src="3" lemma="strong:G2532" morph="robinson:CONJ">and</ns1:w> <ns1:w src="4" lemma="strong:G5560" morph="robinson:A-NPM">the lame</ns1:w> <ns1:w src="5" lemma="strong:G4043" morph="robinson:V-PAI-3P">walk</ns1:w>, <ns1:w src="6" lemma="strong:G3015" morph="robinson:A-NPM">the lepers</ns1:w> <ns1:w src="7" lemma="strong:G2511" morph="robinson:V-PPI-3P">are cleansed</ns1:w>, <ns1:w src="8" lemma="strong:G2532" morph="robinson:CONJ">and</ns1:w> <ns1:w src="9" lemma="strong:G2974" morph="robinson:A-NPM">the deaf</ns1:w> <ns1:w src="10" lemma="strong:G191" morph="robinson:V-PAI-3P">hear</ns1:w>, <ns1:w src="11" lemma="strong:G3498" morph="robinson:A-NPM">the dead</ns1:w> <ns1:w src="12" lemma="strong:G1453" morph="robinson:V-PPI-3P">are raised up</ns1:w>, <ns1:w src="13" lemma="strong:G2532" morph="robinson:CONJ">and</ns1:w> <ns1:w src="14" lemma="strong:G4434" morph="robinson:A-NPM">the poor</ns1:w> <ns1:w src="15" lemma="strong:G2097" morph="robinson:V-PPI-3P">have the gospel preached to them</ns1:w>.<ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-02-06:13" /><ns1:verse eID="Matt.11.5" />
<ns1:verse osisID="Matt.11.6" sID="Matt.11.6" /><ns1:w src="1" lemma="strong:G2532" morph="robinson:CONJ">And</ns1:w> <ns1:w src="2" lemma="strong:G3107" morph="robinson:A-NSM">blessed</ns1:w> <ns1:w src="3" lemma="strong:G2076" morph="robinson:V-PXI-3S">is</ns1:w> <ns1:transChange type="added">he</ns1:transChange>, <ns1:w src="4" lemma="strong:G3739" morph="robinson:R-NSM">who</ns1:w><ns1:w src="5" lemma="strong:G1437" morph="robinson:COND">soever</ns1:w> <ns1:w src="7" lemma="strong:G4624" morph="robinson:V-APS-3S">shall</ns1:w> <ns1:w src="6" lemma="strong:G3361" morph="robinson:PRT-N">not</ns1:w> <ns1:w src="7" lemma="strong:G4624" morph="robinson:V-APS-3S" type="x-split" subType="x-10">be offended</ns1:w> <ns1:w src="8" lemma="strong:G1722" morph="robinson:PREP">in</ns1:w> <ns1:w src="9" lemma="strong:G1698" morph="robinson:P-1DS">me</ns1:w>.</ns1:q><ns1:note type="x-strongsMarkup" resp="ank 2003-12-02-06:16">How to scan "ean" in this context might be a matter for debate. There's either a scribal mistake there and it should be "hos an" for "whoever". Or the "literal" scan should be "he-who if he should not be offended" I chose to match hos with "he whosoever" and let the ean be unmatched in this context. otoh, the strong's dictionary (which would have been strongly influenced by verses like this, seems to allow the ean to be whosoever. I guess I should let it stand that way. In failing to respect the discourse differences between Greek and English it's weaker but there it is… blech, but it's teamwork. And then, I came back and changed my mind after reading the guidelines and deciding I hadn't been following them.:)</ns1:note><ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-02-06:16" /><ns1:verse eID="Matt.11.6" />
<ns1:verse osisID="Matt.11.7" sID="Matt.11.7" /><ns1:milestone type="x-p" marker="¶" /><ns1:w src="2" lemma="strong:G1161" morph="robinson:CONJ">And as</ns1:w> <ns1:w src="1" lemma="strong:G5130" morph="robinson:D-GPM">they</ns1:w> <ns1:w src="3" lemma="strong:G4198" morph="robinson:V-PNP-GPM">departed</ns1:w>, <ns1:w src="5 6" lemma="strong:G3588 strong:G2424" morph="robinson:T-NSM robinson:N-NSM">Jesus</ns1:w> <ns1:w src="4" lemma="strong:G756" morph="robinson:V-ADI-3S">began</ns1:w> <ns1:w src="7" lemma="strong:G3004" morph="robinson:V-PAN">to say</ns1:w> <ns1:w src="8 9" lemma="strong:G3588 strong:G3793" morph="robinson:T-DPM robinson:N-DPM">unto the multitudes</ns1:w> <ns1:w src="10" lemma="strong:G4012" morph="robinson:PREP">concerning</ns1:w> <ns1:w src="11" lemma="strong:G2491" morph="robinson:N-GSM">John</ns1:w>, <ns1:q who="Jesus" marker=""><ns1:w src="12" lemma="strong:G5101" morph="robinson:I-ASN">What</ns1:w> <ns1:w src="13" lemma="strong:G1831" morph="robinson:V-2AAI-2P">went ye out</ns1:w> <ns1:w src="14" lemma="strong:G1519" morph="robinson:PREP">into</ns1:w> <ns1:w src="15" lemma="strong:G3588" morph="robinson:T-ASF" /><ns1:w src="16" lemma="strong:G2048" morph="robinson:A-ASF">the wilderness</ns1:w> <ns1:w src="17" lemma="strong:G2300" morph="robinson:V-ADN">to see</ns1:w>? <ns1:w src="18" lemma="strong:G2563" morph="robinson:N-ASM">A reed</ns1:w> <ns1:w src="21" lemma="strong:G4531" morph="robinson:V-PPP-ASM">shaken</ns1:w> <ns1:w src="19" lemma="strong:G5259" morph="robinson:PREP">with</ns1:w> <ns1:w src="20" lemma="strong:G417" morph="robinson:N-GSM">the wind</ns1:w>?<ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-21-05:58" /><ns1:verse eID="Matt.11.7" />
<ns1:verse osisID="Matt.11.8" sID="Matt.11.8" /><ns1:w src="1" lemma="strong:G235" morph="robinson:CONJ">But</ns1:w> <ns1:w src="2" lemma="strong:G5101" morph="robinson:I-ASN">what</ns1:w> <ns1:w src="3" lemma="strong:G1831" morph="robinson:V-2AAI-2P">went ye out</ns1:w> <ns1:w src="4" lemma="strong:G1492" morph="robinson:V-2AAN">for to see</ns1:w>? <ns1:w src="5" lemma="strong:G444" morph="robinson:N-ASM">A man</ns1:w> <ns1:w src="9" lemma="strong:G294" morph="robinson:V-RPP-ASM">clothed</ns1:w> <ns1:w src="6" lemma="strong:G1722" morph="robinson:PREP">in</ns1:w> <ns1:w src="7" lemma="strong:G3120" morph="robinson:A-DPN">soft</ns1:w> <ns1:w src="8" lemma="strong:G2440" morph="robinson:N-DPN">raiment</ns1:w>? <ns1:w src="10" lemma="strong:G2400" morph="robinson:V-2AAM-2S">behold</ns1:w>, <ns1:w src="11" lemma="strong:G3588" morph="robinson:T-NPM">they that</ns1:w> <ns1:w src="14" lemma="strong:G5409" morph="robinson:V-PAP-NPM">wear</ns1:w> <ns1:w src="12" lemma="strong:G3588" morph="robinson:T-APN" /><ns1:w src="13" lemma="strong:G3120" morph="robinson:A-APN">soft</ns1:w> <ns1:transChange type="added">clothing</ns1:transChange> <ns1:w src="20" lemma="strong:G1526" morph="robinson:V-PXI-3P">are</ns1:w> <ns1:w src="15" lemma="strong:G1722" morph="robinson:PREP">in</ns1:w> <ns1:w src="18 19" lemma="strong:G3588 strong:G935" morph="robinson:T-GPM robinson:N-GPM">kings'</ns1:w> <ns1:w src="16 17" lemma="strong:G3588 strong:G3624" morph="robinson:T-DPM robinson:N-DPM">houses</ns1:w>.<ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-21-05:59" /><ns1:verse eID="Matt.11.8" />
<ns1:verse osisID="Matt.11.9" sID="Matt.11.9" /><ns1:w src="1" lemma="strong:G235" morph="robinson:CONJ">But</ns1:w> <ns1:w src="2" lemma="strong:G5101" morph="robinson:I-ASN">what</ns1:w> <ns1:w src="3" lemma="strong:G1831" morph="robinson:V-2AAI-2P">went ye out</ns1:w> <ns1:w src="4" lemma="strong:G1492" morph="robinson:V-2AAN">for to see</ns1:w>? <ns1:w src="5" lemma="strong:G4396" morph="robinson:N-ASM">A prophet</ns1:w>? <ns1:w src="6" lemma="strong:G3483" morph="robinson:PRT">yea</ns1:w>, <ns1:w src="7" lemma="strong:G3004" morph="robinson:V-PAI-1S">I say</ns1:w> <ns1:w src="8" lemma="strong:G5213" morph="robinson:P-2DP">unto you</ns1:w>, <ns1:w src="9" lemma="strong:G2532" morph="robinson:CONJ">and</ns1:w> <ns1:w src="10" lemma="strong:G4053" morph="robinson:A-NSN-C">more than</ns1:w> <ns1:w src="11" lemma="strong:G4396" morph="robinson:N-GSM">a prophet</ns1:w>.<ns1:milestone type="x-strongsMarkup" resp="ank 2003-12-21-05:59" /><ns1:verse eID="Matt.11.9" />
