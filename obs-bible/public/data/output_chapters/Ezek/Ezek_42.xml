<ns1:verse osisID="Ezek.42.3" sID="Ezek.42.3" /><ns1:w lemma="strong:H06242">Over against the twenty</ns1:w> <ns1:transChange type="added">cubits</ns1:transChange> which <ns1:transChange type="added">were</ns1:transChange> <ns1:w lemma="strong:H06442">for the inner</ns1:w> <ns1:w lemma="strong:H02691">court</ns1:w>, <ns1:w lemma="strong:H07531">and over against the pavement</ns1:w> which <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H02435">for the utter</ns1:w> <ns1:w lemma="strong:H02691">court</ns1:w>, <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H0862">gallery</ns1:w> <ns1:w lemma="strong:H06440">against</ns1:w> <ns1:w lemma="strong:H0862">gallery</ns1:w> <ns1:w lemma="strong:H07992">in three</ns1:w> <ns1:transChange type="added">stories</ns1:transChange>.<ns1:verse eID="Ezek.42.3" />
