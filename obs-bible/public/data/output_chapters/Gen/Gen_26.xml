<ns1:verse osisID="Gen.26.3" sID="Gen.26.3" /><ns1:w morph="strongMorph:TH8798" lemma="strong:H01481">Sojourn</ns1:w> <ns1:w lemma="strong:H0776">in this land</ns1:w>, <ns1:w morph="strongMorph:TH8762" lemma="strong:H01288">and I will be with thee, and will bless</ns1:w> <ns1:w lemma="strong:H02233">thee; for unto thee, and unto thy seed</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H05414">I will give</ns1:w> <ns1:w lemma="strong:H0411">all these</ns1:w> <ns1:w lemma="strong:H0776">countries</ns1:w>, <ns1:w morph="strongMorph:TH8689" lemma="strong:H06965">and I will perform</ns1:w> <ns1:w lemma="strong:H07621">the oath</ns1:w> <ns1:w morph="strongMorph:TH8738" lemma="strong:H07650">which I sware</ns1:w> <ns1:w lemma="strong:H085">unto Abraham</ns1:w> <ns1:w lemma="strong:H01">thy father</ns1:w>;<ns1:verse eID="Gen.26.3" />
<ns1:verse osisID="Gen.26.4" sID="Gen.26.4" /><ns1:w lemma="strong:H07235">And I will make</ns1:w> <ns1:w lemma="strong:H02233">thy seed</ns1:w> <ns1:w morph="strongMorph:TH8689" lemma="strong:H07235">to multiply</ns1:w> <ns1:w lemma="strong:H03556">as the stars</ns1:w> <ns1:w lemma="strong:H08064">of heaven</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H05414">and will give</ns1:w> <ns1:w lemma="strong:H02233">unto thy seed</ns1:w> <ns1:w lemma="strong:H0411">all these</ns1:w> <ns1:w lemma="strong:H0776">countries</ns1:w>; <ns1:w lemma="strong:H02233">and in thy seed</ns1:w> <ns1:w lemma="strong:H01471">shall all the nations</ns1:w> <ns1:w lemma="strong:H0776">of the earth</ns1:w> <ns1:w morph="strongMorph:TH8694" lemma="strong:H01288">be blessed</ns1:w>;<ns1:verse eID="Gen.26.4" />
