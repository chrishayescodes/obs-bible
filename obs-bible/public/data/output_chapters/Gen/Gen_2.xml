<ns1:verse osisID="Gen.2.23" sID="Gen.2.23" /><ns1:w lemma="strong:H0120">And Adam</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w>, <ns1:w lemma="strong:H02063">This</ns1:w> <ns1:transChange type="added">is</ns1:transChange> <ns1:w lemma="strong:H06471">now</ns1:w> <ns1:w lemma="strong:H06106">bone</ns1:w> <ns1:w lemma="strong:H06106">of my bones</ns1:w>, <ns1:w lemma="strong:H01320">and flesh</ns1:w> <ns1:w lemma="strong:H01320">of my flesh</ns1:w>: <ns1:w lemma="strong:H02063">she</ns1:w> <ns1:w morph="strongMorph:TH8735" lemma="strong:H07121">shall be called</ns1:w> <ns1:w lemma="strong:H0802">Woman</ns1:w>, <ns1:w lemma="strong:H02063">because she</ns1:w> <ns1:w morph="strongMorph:TH8795" lemma="strong:H03947">was taken</ns1:w> <ns1:w lemma="strong:H0376">out of Man</ns1:w>.<ns1:note type="study">Woman: Heb. Isha</ns1:note><ns1:note type="study">Man: Heb. Ish</ns1:note><ns1:verse eID="Gen.2.23" />
<ns1:verse osisID="Gen.2.24" sID="Gen.2.24" /><ns1:w lemma="strong:H03651">Therefore</ns1:w> <ns1:w lemma="strong:H0376">shall a man</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H05800">leave</ns1:w> <ns1:w lemma="strong:H01">his father</ns1:w> <ns1:w lemma="strong:H0517">and his mother</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H01692">and shall cleave</ns1:w> <ns1:w lemma="strong:H0802">unto his wife</ns1:w>: <ns1:w lemma="strong:H0259">and they shall be one</ns1:w> <ns1:w lemma="strong:H01320">flesh</ns1:w>.<ns1:verse eID="Gen.2.24" />
<ns1:verse osisID="Gen.2.25" sID="Gen.2.25" /><ns1:w lemma="strong:H08147">And they were both</ns1:w> <ns1:w lemma="strong:H06174">naked</ns1:w>, <ns1:w lemma="strong:H0120">the man</ns1:w> <ns1:w lemma="strong:H0802">and his wife</ns1:w>, <ns1:w morph="strongMorph:TH8709" lemma="strong:H0954">and were not ashamed</ns1:w>.<ns1:verse eID="Gen.2.25" />
</ns1:chapter>
