<ns1:verse osisID="Gen.41.49" sID="Gen.41.49" /><ns1:w lemma="strong:H03130">And Joseph</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06651">gathered</ns1:w> <ns1:w lemma="strong:H01250">corn</ns1:w> <ns1:w lemma="strong:H02344">as the sand</ns1:w> <ns1:w lemma="strong:H03220">of the sea</ns1:w>, <ns1:w lemma="strong:H03966">very</ns1:w> <ns1:w morph="strongMorph:TH8687" lemma="strong:H07235">much</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H02308">until he left</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H05608">numbering</ns1:w>; for <ns1:transChange type="added">it was</ns1:transChange> <ns1:w lemma="strong:H0369">without</ns1:w> <ns1:w lemma="strong:H04557">number</ns1:w>.<ns1:verse eID="Gen.41.49" />
<ns1:verse osisID="Gen.41.50" sID="Gen.41.50" /><ns1:w lemma="strong:H03130">And unto Joseph</ns1:w> <ns1:w morph="strongMorph:TH8795" lemma="strong:H03205">were born</ns1:w> <ns1:w lemma="strong:H08147">two</ns1:w> <ns1:w lemma="strong:H01121">sons</ns1:w> <ns1:w lemma="strong:H08141">before the years</ns1:w> <ns1:w lemma="strong:H07458">of famine</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0935">came</ns1:w>, <ns1:w lemma="strong:H0621">which Asenath</ns1:w> <ns1:w lemma="strong:H01323">the daughter</ns1:w> <ns1:w lemma="strong:H06319">of Poti–pherah</ns1:w> <ns1:w lemma="strong:H03548">priest</ns1:w> <ns1:w lemma="strong:H0204">of On</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H03205">bare</ns1:w> unto him.<ns1:note type="study">priest: or, prince</ns1:note><ns1:verse eID="Gen.41.50" />
<ns1:verse osisID="Gen.41.51" sID="Gen.41.51" /><ns1:w lemma="strong:H03130">And Joseph</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H07121">called</ns1:w> <ns1:w lemma="strong:H08034">the name</ns1:w> <ns1:w lemma="strong:H01060">of the firstborn</ns1:w> <ns1:w lemma="strong:H04519">Manasseh</ns1:w>: <ns1:w lemma="strong:H0430">For God</ns1:w>, <ns1:transChange type="added">said he</ns1:transChange>, <ns1:w morph="strongMorph:TH8765" lemma="strong:H05382">hath made me forget</ns1:w> <ns1:w lemma="strong:H05999">all my toil</ns1:w>, <ns1:w lemma="strong:H01">and all my father's</ns1:w> <ns1:w lemma="strong:H01004">house</ns1:w>.<ns1:note type="study">Manasseh: that is, Forgetting</ns1:note><ns1:verse eID="Gen.41.51" />
