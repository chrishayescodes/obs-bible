<ns1:verse osisID="Gen.36.20" sID="Gen.36.20" /><ns1:milestone type="x-p" marker="¶" />These <ns1:transChange type="added">are</ns1:transChange> <ns1:w lemma="strong:H01121">the sons</ns1:w> <ns1:w lemma="strong:H08165">of Seir</ns1:w> <ns1:w lemma="strong:H02752">the Horite</ns1:w>, <ns1:w morph="strongMorph:TH8802" lemma="strong:H03427">who inhabited</ns1:w> <ns1:w lemma="strong:H0776">the land</ns1:w>; <ns1:w lemma="strong:H03877">Lotan</ns1:w>, <ns1:w lemma="strong:H07732">and Shobal</ns1:w>, <ns1:w lemma="strong:H06649">and Zibeon</ns1:w>, <ns1:w lemma="strong:H06034">and Anah</ns1:w>,<ns1:verse eID="Gen.36.20" />
