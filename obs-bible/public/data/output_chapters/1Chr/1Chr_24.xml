<ns1:verse osisID="1Chr.24.7" sID="1Chr.24.7" /><ns1:w lemma="strong:H07223">Now the first</ns1:w> <ns1:w lemma="strong:H01486">lot</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H03318">came forth</ns1:w> <ns1:w lemma="strong:H03080">to Jehoiarib</ns1:w>, <ns1:w lemma="strong:H08145">the second</ns1:w> <ns1:w lemma="strong:H03048">to Jedaiah</ns1:w>,<ns1:verse eID="1Chr.24.7" />
