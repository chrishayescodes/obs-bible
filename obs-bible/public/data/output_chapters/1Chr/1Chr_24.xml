<ns1:verse osisID="1Chr.24.11" sID="1Chr.24.11" /><ns1:w lemma="strong:H08671">The ninth</ns1:w> <ns1:w lemma="strong:H03442">to Jeshua</ns1:w>, <ns1:w lemma="strong:H06224">the tenth</ns1:w> <ns1:w lemma="strong:H07935">to Shecaniah</ns1:w>,<ns1:verse eID="1Chr.24.11" />
