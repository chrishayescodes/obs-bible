<ns1:verse osisID="1Chr.24.9" sID="1Chr.24.9" /><ns1:w lemma="strong:H02549">The fifth</ns1:w> <ns1:w lemma="strong:H04441">to Malchijah</ns1:w>, <ns1:w lemma="strong:H08345">the sixth</ns1:w> <ns1:w lemma="strong:H04326">to Mijamin</ns1:w>,<ns1:verse eID="1Chr.24.9" />
