<ns1:verse osisID="1Chr.29.16" sID="1Chr.29.16" /><ns1:w lemma="strong:H03068">O <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H0430">our God</ns1:w>, <ns1:w lemma="strong:H01995">all this store</ns1:w> <ns1:w morph="strongMorph:TH8689" lemma="strong:H03559">that we have prepared</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H01129">to build</ns1:w> <ns1:w lemma="strong:H01004">thee an house</ns1:w> <ns1:w lemma="strong:H06944">for thine holy</ns1:w> <ns1:w lemma="strong:H08034">name</ns1:w> <ns1:transChange type="added">cometh</ns1:transChange> <ns1:w lemma="strong:H03027">of thine hand</ns1:w>, and <ns1:transChange type="added">is</ns1:transChange> all thine own.<ns1:verse eID="1Chr.29.16" />
<ns1:verse osisID="1Chr.29.17" sID="1Chr.29.17" /><ns1:w morph="strongMorph:TH8804" lemma="strong:H03045">I know</ns1:w> <ns1:w lemma="strong:H0430">also, my God</ns1:w>, <ns1:w morph="strongMorph:TH8802" lemma="strong:H0974">that thou triest</ns1:w> <ns1:w lemma="strong:H03824">the heart</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H07521">and hast pleasure</ns1:w> <ns1:w lemma="strong:H03476">in uprightness</ns1:w>. <ns1:w lemma="strong:H04339">As for me, in the uprightness</ns1:w> <ns1:w lemma="strong:H03824">of mine heart</ns1:w> <ns1:w morph="strongMorph:TH8694" lemma="strong:H05068">I have willingly offered</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H07200">all these things: and now have I seen</ns1:w> <ns1:w lemma="strong:H08057">with joy</ns1:w> <ns1:w lemma="strong:H05971">thy people</ns1:w>, <ns1:w morph="strongMorph:TH8738" lemma="strong:H04672">which are present</ns1:w> <ns1:w morph="strongMorph:TH8692" lemma="strong:H05068">here, to offer willingly</ns1:w> unto thee.<ns1:note type="study">present: Heb. found</ns1:note><ns1:verse eID="1Chr.29.17" />
<ns1:verse osisID="1Chr.29.18" sID="1Chr.29.18" /><ns1:w lemma="strong:H03068">O <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:w lemma="strong:H0430">God</ns1:w> <ns1:w lemma="strong:H085">of Abraham</ns1:w>, <ns1:w lemma="strong:H03327">Isaac</ns1:w>, <ns1:w lemma="strong:H03478">and of Israel</ns1:w>, <ns1:w lemma="strong:H01">our fathers</ns1:w>, <ns1:w morph="strongMorph:TH8798" lemma="strong:H08104">keep</ns1:w> <ns1:w lemma="strong:H05769">this for ever</ns1:w> <ns1:w lemma="strong:H03336">in the imagination</ns1:w> <ns1:w lemma="strong:H04284">of the thoughts</ns1:w> <ns1:w lemma="strong:H03824">of the heart</ns1:w> <ns1:w lemma="strong:H05971">of thy people</ns1:w>, <ns1:w morph="strongMorph:TH8685" lemma="strong:H03559">and prepare</ns1:w> <ns1:w lemma="strong:H03824">their heart</ns1:w> unto thee:<ns1:note type="study">prepare: or, stablish</ns1:note><ns1:verse eID="1Chr.29.18" />
