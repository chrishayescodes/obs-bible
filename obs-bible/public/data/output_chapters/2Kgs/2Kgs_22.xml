<ns1:verse osisID="2Kgs.22.1" sID="2Kgs.22.1" /><ns1:w lemma="strong:H02977">Josiah</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H08083">eight</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w> <ns1:w lemma="strong:H01121">old</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H04427">when he began to reign</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H04427">and he reigned</ns1:w> <ns1:w lemma="strong:H07970">thirty</ns1:w> <ns1:w lemma="strong:H0259">and one</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w> <ns1:w lemma="strong:H03389">in Jerusalem</ns1:w>. <ns1:w lemma="strong:H0517">And his mother's</ns1:w> <ns1:w lemma="strong:H08034">name</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H03040">Jedidah</ns1:w>, <ns1:w lemma="strong:H01323">the daughter</ns1:w> <ns1:w lemma="strong:H05718">of Adaiah</ns1:w> <ns1:w lemma="strong:H01218">of Boscath</ns1:w>.<ns1:verse eID="2Kgs.22.1" />
<ns1:verse osisID="2Kgs.22.2" sID="2Kgs.22.2" /><ns1:w morph="strongMorph:TH8799" lemma="strong:H06213">And he did</ns1:w> <ns1:transChange type="added">that which was</ns1:transChange> <ns1:w lemma="strong:H03477">right</ns1:w> <ns1:w lemma="strong:H05869">in the sight</ns1:w> <ns1:w lemma="strong:H03068">of the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H03212">and walked</ns1:w> <ns1:w lemma="strong:H01870">in all the way</ns1:w> <ns1:w lemma="strong:H01732">of David</ns1:w> <ns1:w lemma="strong:H01">his father</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H05493">and turned not aside</ns1:w> <ns1:w lemma="strong:H03225">to the right hand</ns1:w> <ns1:w lemma="strong:H08040">or to the left</ns1:w>.<ns1:verse eID="2Kgs.22.2" />
