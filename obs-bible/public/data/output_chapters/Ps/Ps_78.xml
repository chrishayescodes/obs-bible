<ns1:verse osisID="Ps.78.12" sID="Ps.78.12" /><ns1:w lemma="strong:H06382">Marvellous things</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H06213">did</ns1:w> <ns1:w lemma="strong:H05048">he in the sight</ns1:w> <ns1:w lemma="strong:H01">of their fathers</ns1:w>, <ns1:w lemma="strong:H0776">in the land</ns1:w> <ns1:w lemma="strong:H04714">of Egypt</ns1:w>, <ns1:transChange type="added">in</ns1:transChange> <ns1:w lemma="strong:H07704">the field</ns1:w> <ns1:w lemma="strong:H06814">of Zoan</ns1:w>.<ns1:verse eID="Ps.78.12" />
