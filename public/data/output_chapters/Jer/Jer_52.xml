<ns1:verse osisID="Jer.52.1" sID="Jer.52.1" /><ns1:w lemma="strong:H06667">Zedekiah</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H0259">one</ns1:w> <ns1:w lemma="strong:H06242">and twenty</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w> <ns1:w lemma="strong:H01121">old</ns1:w> <ns1:w morph="strongMorph:TH8800" lemma="strong:H04427">when he began to reign</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H04427">and he reigned</ns1:w> <ns1:w lemma="strong:H06240 strong:H0259">eleven</ns1:w> <ns1:w lemma="strong:H08141">years</ns1:w> <ns1:w lemma="strong:H03389">in Jerusalem</ns1:w>. <ns1:w lemma="strong:H0517">And his mother's</ns1:w> <ns1:w lemma="strong:H08034">name</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H02537">Hamutal</ns1:w> <ns1:w lemma="strong:H01323">the daughter</ns1:w> <ns1:w lemma="strong:H03414">of Jeremiah</ns1:w> <ns1:w lemma="strong:H03841">of Libnah</ns1:w>.<ns1:note type="study">began…: Heb. reigned</ns1:note><ns1:verse eID="Jer.52.1" />
