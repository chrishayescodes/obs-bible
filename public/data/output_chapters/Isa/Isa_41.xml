<ns1:verse osisID="Isa.41.1" sID="Isa.41.1" /><ns1:w morph="strongMorph:TH8685" lemma="strong:H02790">Keep silence</ns1:w> <ns1:w lemma="strong:H0339">before me, O islands</ns1:w>; <ns1:w lemma="strong:H03816">and let the people</ns1:w> <ns1:w morph="strongMorph:TH8686" lemma="strong:H02498">renew</ns1:w> <ns1:transChange type="added">their</ns1:transChange> <ns1:w lemma="strong:H03581">strength</ns1:w>: <ns1:w morph="strongMorph:TH8799" lemma="strong:H05066">let them come near</ns1:w>; <ns1:w morph="strongMorph:TH8762" lemma="strong:H01696">then let them speak</ns1:w>: <ns1:w morph="strongMorph:TH8799" lemma="strong:H07126">let us come near</ns1:w> <ns1:w lemma="strong:H03162">together</ns1:w> <ns1:w lemma="strong:H04941">to judgment</ns1:w>.<ns1:verse eID="Isa.41.1" />
