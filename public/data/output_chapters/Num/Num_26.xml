<ns1:verse osisID="Num.26.3" sID="Num.26.3" /><ns1:w lemma="strong:H04872">And Moses</ns1:w> <ns1:w lemma="strong:H0499">and Eleazar</ns1:w> <ns1:w lemma="strong:H03548">the priest</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01696">spake</ns1:w> <ns1:w lemma="strong:H06160">with them in the plains</ns1:w> <ns1:w lemma="strong:H04124">of Moab</ns1:w> <ns1:w lemma="strong:H03383">by Jordan</ns1:w> <ns1:transChange type="added">near</ns1:transChange> <ns1:w lemma="strong:H03405">Jericho</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>,<ns1:verse eID="Num.26.3" />
