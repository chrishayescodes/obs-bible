<ns1:verse osisID="Num.23.1" sID="Num.23.1" /><ns1:w lemma="strong:H01109">And Balaam</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w> <ns1:w lemma="strong:H01111">unto Balak</ns1:w>, <ns1:w morph="strongMorph:TH8798" lemma="strong:H01129">Build</ns1:w> <ns1:w lemma="strong:H07651">me here seven</ns1:w> <ns1:w lemma="strong:H04196">altars</ns1:w>, <ns1:w morph="strongMorph:TH8685" lemma="strong:H03559">and prepare</ns1:w> <ns1:w lemma="strong:H07651">me here seven</ns1:w> <ns1:w lemma="strong:H06499">oxen</ns1:w> <ns1:w lemma="strong:H07651">and seven</ns1:w> <ns1:w lemma="strong:H0352">rams</ns1:w>.<ns1:verse eID="Num.23.1" />
