<ns1:verse osisID="2Chr.34.20" sID="2Chr.34.20" /><ns1:w lemma="strong:H04428">And the king</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H06680">commanded</ns1:w> <ns1:w lemma="strong:H02518">Hilkiah</ns1:w>, <ns1:w lemma="strong:H0296">and Ahikam</ns1:w> <ns1:w lemma="strong:H01121">the son</ns1:w> <ns1:w lemma="strong:H08227">of Shaphan</ns1:w>, <ns1:w lemma="strong:H05658">and Abdon</ns1:w> <ns1:w lemma="strong:H01121">the son</ns1:w> <ns1:w lemma="strong:H04318">of Micah</ns1:w>, <ns1:w lemma="strong:H08227">and Shaphan</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H05608">the scribe</ns1:w>, <ns1:w lemma="strong:H06222">and Asaiah</ns1:w> <ns1:w lemma="strong:H05650">a servant</ns1:w> <ns1:w lemma="strong:H04428">of the king's</ns1:w>, <ns1:w morph="strongMorph:TH8800" lemma="strong:H0559">saying</ns1:w>,<ns1:note type="study">Abdon: or, Achbor</ns1:note><ns1:verse eID="2Chr.34.20" />
