<ns1:verse osisID="Ezek.13.13" sID="Ezek.13.13" /><ns1:w morph="strongMorph:TH8804" lemma="strong:H0559">Therefore thus saith</ns1:w> <ns1:w lemma="strong:H0136">the Lord</ns1:w> <ns1:w lemma="strong:H03069"><ns1:seg><ns1:divineName>God</ns1:divineName></ns1:seg></ns1:w>; <ns1:w morph="strongMorph:TH8765" lemma="strong:H01234">I will even rend</ns1:w> <ns1:transChange type="added">it</ns1:transChange> <ns1:w lemma="strong:H05591">with a stormy</ns1:w> <ns1:w lemma="strong:H07307">wind</ns1:w> <ns1:w lemma="strong:H02534">in my fury</ns1:w>; <ns1:w morph="strongMorph:TH8802" lemma="strong:H07857">and there shall be an overflowing</ns1:w> <ns1:w lemma="strong:H01653">shower</ns1:w> <ns1:w lemma="strong:H0639">in mine anger</ns1:w>, <ns1:w lemma="strong:H068 strong:H0417">and great hailstones</ns1:w> in <ns1:transChange type="added">my</ns1:transChange> <ns1:w lemma="strong:H02534">fury</ns1:w> <ns1:w lemma="strong:H03617">to consume</ns1:w> <ns1:transChange type="added">it</ns1:transChange>.<ns1:verse eID="Ezek.13.13" />
<ns1:verse osisID="Ezek.13.14" sID="Ezek.13.14" /><ns1:w morph="strongMorph:TH8804" lemma="strong:H02040">So will I break down</ns1:w> <ns1:w lemma="strong:H07023">the wall</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H02902">that ye have daubed</ns1:w> <ns1:w lemma="strong:H08602">with untempered</ns1:w> <ns1:transChange type="added">morter</ns1:transChange>, <ns1:w morph="strongMorph:TH8689" lemma="strong:H05060">and bring</ns1:w> <ns1:w lemma="strong:H0776">it down to the ground</ns1:w>, <ns1:w lemma="strong:H03247">so that the foundation</ns1:w> <ns1:w morph="strongMorph:TH8738" lemma="strong:H01540">thereof shall be discovered</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H05307">and it shall fall</ns1:w>, <ns1:w morph="strongMorph:TH8804" lemma="strong:H03615">and ye shall be consumed</ns1:w> <ns1:w lemma="strong:H08432">in the midst</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H03045">thereof: and ye shall know</ns1:w> that I <ns1:transChange type="added">am</ns1:transChange> <ns1:w lemma="strong:H03068">the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w>.<ns1:verse eID="Ezek.13.14" />
