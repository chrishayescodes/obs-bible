<ns1:verse osisID="Gen.25.31" sID="Gen.25.31" /><ns1:w lemma="strong:H03290">And Jacob</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w>, <ns1:w morph="strongMorph:TH8798" lemma="strong:H04376">Sell</ns1:w> <ns1:w lemma="strong:H03117">me this day</ns1:w> <ns1:w lemma="strong:H01062">thy birthright</ns1:w>.<ns1:verse eID="Gen.25.31" />
