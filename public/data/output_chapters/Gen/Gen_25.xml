<ns1:verse osisID="Gen.25.33" sID="Gen.25.33" /><ns1:w lemma="strong:H03290">And Jacob</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H0559">said</ns1:w>, <ns1:w morph="strongMorph:TH8734" lemma="strong:H07650">Swear</ns1:w> <ns1:w lemma="strong:H03117">to me this day</ns1:w>; <ns1:w morph="strongMorph:TH8735" lemma="strong:H07650">and he sware</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H04376">unto him: and he sold</ns1:w> <ns1:w lemma="strong:H01062">his birthright</ns1:w> <ns1:w lemma="strong:H03290">unto Jacob</ns1:w>.<ns1:verse eID="Gen.25.33" />
