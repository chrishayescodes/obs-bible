<ns1:verse osisID="Gen.25.9" sID="Gen.25.9" /><ns1:w lemma="strong:H01121">And his sons</ns1:w> <ns1:w lemma="strong:H03327">Isaac</ns1:w> <ns1:w lemma="strong:H03458">and Ishmael</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H06912">buried</ns1:w> <ns1:w lemma="strong:H04631">him in the cave</ns1:w> <ns1:w lemma="strong:H04375">of Machpelah</ns1:w>, <ns1:w lemma="strong:H07704">in the field</ns1:w> <ns1:w lemma="strong:H06085">of Ephron</ns1:w> <ns1:w lemma="strong:H01121">the son</ns1:w> <ns1:w lemma="strong:H06714">of Zohar</ns1:w> <ns1:w lemma="strong:H02850">the Hittite</ns1:w>, which <ns1:transChange type="added">is</ns1:transChange> <ns1:w lemma="strong:H06440">before</ns1:w> <ns1:w lemma="strong:H04471">Mamre</ns1:w>;<ns1:verse eID="Gen.25.9" />
