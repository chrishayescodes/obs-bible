<ns1:verse osisID="Gen.22.21" sID="Gen.22.21" /><ns1:w lemma="strong:H05780">Huz</ns1:w> <ns1:w lemma="strong:H01060">his firstborn</ns1:w>, <ns1:w lemma="strong:H0938">and Buz</ns1:w> <ns1:w lemma="strong:H0251">his brother</ns1:w>, <ns1:w lemma="strong:H07055">and Kemuel</ns1:w> <ns1:w lemma="strong:H01">the father</ns1:w> <ns1:w lemma="strong:H0758">of Aram</ns1:w>,<ns1:verse eID="Gen.22.21" />
<ns1:verse osisID="Gen.22.22" sID="Gen.22.22" /><ns1:w lemma="strong:H03777">And Chesed</ns1:w>, <ns1:w lemma="strong:H02375">and Hazo</ns1:w>, <ns1:w lemma="strong:H06394">and Pildash</ns1:w>, <ns1:w lemma="strong:H03044">and Jidlaph</ns1:w>, <ns1:w lemma="strong:H01328">and Bethuel</ns1:w>.<ns1:verse eID="Gen.22.22" />
<ns1:verse osisID="Gen.22.23" sID="Gen.22.23" /><ns1:w lemma="strong:H01328">And Bethuel</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H03205">begat</ns1:w> <ns1:w lemma="strong:H07259">Rebekah</ns1:w>: <ns1:w lemma="strong:H08083">these eight</ns1:w> <ns1:w lemma="strong:H04435">Milcah</ns1:w> <ns1:w morph="strongMorph:TH8804" lemma="strong:H03205">did bear</ns1:w> <ns1:w lemma="strong:H05152">to Nahor</ns1:w>, <ns1:w lemma="strong:H085">Abraham's</ns1:w> <ns1:w lemma="strong:H0251">brother</ns1:w>.<ns1:note type="study">Rebekah: Gr. Rebecca</ns1:note><ns1:verse eID="Gen.22.23" />
<ns1:verse osisID="Gen.22.24" sID="Gen.22.24" /><ns1:w lemma="strong:H06370">And his concubine</ns1:w>, <ns1:w lemma="strong:H08034">whose name</ns1:w> <ns1:transChange type="added">was</ns1:transChange> <ns1:w lemma="strong:H07208">Reumah</ns1:w>, <ns1:w morph="strongMorph:TH8799" lemma="strong:H03205">she bare</ns1:w> <ns1:w lemma="strong:H02875">also Tebah</ns1:w>, <ns1:w lemma="strong:H01514">and Gaham</ns1:w>, <ns1:w lemma="strong:H08477">and Thahash</ns1:w>, <ns1:w lemma="strong:H04601">and Maachah</ns1:w>.<ns1:verse eID="Gen.22.24" />
</ns1:chapter>
