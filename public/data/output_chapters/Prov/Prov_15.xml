<ns1:verse osisID="Prov.15.30" sID="Prov.15.30" /><ns1:w lemma="strong:H03974">The light</ns1:w> <ns1:w lemma="strong:H05869">of the eyes</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H08055">rejoiceth</ns1:w> <ns1:w lemma="strong:H03820">the heart</ns1:w>: <ns1:transChange type="added">and</ns1:transChange> <ns1:w lemma="strong:H02896">a good</ns1:w> <ns1:w lemma="strong:H08052">report</ns1:w> <ns1:w lemma="strong:H01878">maketh</ns1:w> <ns1:w lemma="strong:H06106">the bones</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01878">fat</ns1:w>.<ns1:verse eID="Prov.15.30" />
<ns1:verse osisID="Prov.15.31" sID="Prov.15.31" /><ns1:w lemma="strong:H0241">The ear</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H08085">that heareth</ns1:w> <ns1:w lemma="strong:H08433">the reproof</ns1:w> <ns1:w lemma="strong:H02416">of life</ns1:w> <ns1:w morph="strongMorph:TH8799" lemma="strong:H03885">abideth</ns1:w> <ns1:w lemma="strong:H07130">among</ns1:w> <ns1:w lemma="strong:H02450">the wise</ns1:w>.<ns1:verse eID="Prov.15.31" />
<ns1:verse osisID="Prov.15.32" sID="Prov.15.32" /><ns1:w morph="strongMorph:TH8802" lemma="strong:H06544">He that refuseth</ns1:w> <ns1:w lemma="strong:H04148">instruction</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H03988">despiseth</ns1:w> <ns1:w lemma="strong:H05315">his own soul</ns1:w>: <ns1:w morph="strongMorph:TH8802" lemma="strong:H08085">but he that heareth</ns1:w> <ns1:w lemma="strong:H08433">reproof</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H07069">getteth</ns1:w> <ns1:w lemma="strong:H03820">understanding</ns1:w>.<ns1:note type="study">instruction: or, correction</ns1:note><ns1:note type="study">heareth: or, obeyeth</ns1:note><ns1:note type="study">getteth…: Heb. possesseth an heart</ns1:note><ns1:verse eID="Prov.15.32" />
<ns1:verse osisID="Prov.15.33" sID="Prov.15.33" /><ns1:w lemma="strong:H03374">The fear</ns1:w> <ns1:w lemma="strong:H03068">of the <ns1:seg><ns1:divineName>Lord</ns1:divineName></ns1:seg></ns1:w> <ns1:transChange type="added">is</ns1:transChange> <ns1:w lemma="strong:H04148">the instruction</ns1:w> <ns1:w lemma="strong:H02451">of wisdom</ns1:w>; <ns1:w lemma="strong:H06440">and before</ns1:w> <ns1:w lemma="strong:H03519">honour</ns1:w> <ns1:transChange type="added">is</ns1:transChange> <ns1:w lemma="strong:H06038">humility</ns1:w>.<ns1:verse eID="Prov.15.33" />
</ns1:chapter>
