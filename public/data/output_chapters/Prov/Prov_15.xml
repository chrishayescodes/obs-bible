<ns1:verse osisID="Prov.15.18" sID="Prov.15.18" /><ns1:w lemma="strong:H02534">A wrathful</ns1:w> <ns1:w lemma="strong:H0376">man</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H01624">stirreth up</ns1:w> <ns1:w lemma="strong:H04066">strife</ns1:w>: but <ns1:transChange type="added">he that is</ns1:transChange> <ns1:w lemma="strong:H0750">slow</ns1:w> <ns1:w lemma="strong:H0639">to anger</ns1:w> <ns1:w morph="strongMorph:TH8686" lemma="strong:H08252">appeaseth</ns1:w> <ns1:w lemma="strong:H07379">strife</ns1:w>.<ns1:verse eID="Prov.15.18" />
