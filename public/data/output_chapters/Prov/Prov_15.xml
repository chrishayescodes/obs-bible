<ns1:verse osisID="Prov.15.20" sID="Prov.15.20" /><ns1:w lemma="strong:H02450">A wise</ns1:w> <ns1:w lemma="strong:H01121">son</ns1:w> <ns1:w morph="strongMorph:TH8762" lemma="strong:H08055">maketh a glad</ns1:w> <ns1:w lemma="strong:H01">father</ns1:w>: <ns1:w lemma="strong:H03684">but a foolish</ns1:w> <ns1:w lemma="strong:H0120">man</ns1:w> <ns1:w morph="strongMorph:TH8802" lemma="strong:H0959">despiseth</ns1:w> <ns1:w lemma="strong:H0517">his mother</ns1:w>.<ns1:verse eID="Prov.15.20" />
